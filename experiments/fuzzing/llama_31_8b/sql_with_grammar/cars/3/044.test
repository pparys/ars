set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE products(id INTEGER, name TEXT, price REAL);
    INSERT INTO products(id, name, price) VALUES(1001, 'Product1', 10.99);
    INSERT INTO products(id, name, price) VALUES(1002, 'Product2', 5.99);
  }
} {}

do_test select-expensive-products-3.1 {
  execsql {
    SELECT name FROM products WHERE price > 10;
  }
} {Product1}

finish_test