set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-1.0 {
  execsql {
    CREATE TABLE students(id INTEGER PRIMARY KEY, name TEXT NOT NULL, age INTEGER NOT NULL);
    INSERT INTO students(id, name, age) VALUES(1, 'John', 20);
    INSERT INTO students(id, name, age) VALUES(2, 'Doe', 21);
  }
} {}

do_test select-student-age-1.1 {
  execsql {
    SELECT age FROM students WHERE name = 'Doe';
  }
} {21}

finish_test