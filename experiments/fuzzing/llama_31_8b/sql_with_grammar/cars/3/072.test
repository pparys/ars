set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE products(product_id INTEGER PRIMARY KEY, name TEXT, price REAL);
    INSERT INTO products(product_id, name, price) VALUES(1001, 'Laptop', 999.99);
    INSERT INTO products(product_id, name, price) VALUES(1002, 'Desktop', 599.99);
    INSERT INTO products(product_id, name, price) VALUES(1003, 'Tablet', 399.99);
  }
} {}

do_test select-product-3.1 {
  execsql {
    SELECT name, price FROM products WHERE price > 600;
  }
} {Desktop 599.99}

finish_test