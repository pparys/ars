set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE books(id INTEGER PRIMARY KEY, title TEXT, author TEXT);
    INSERT INTO books VALUES(1, 'Book 1', 'Author A');
    INSERT INTO books VALUES(2, 'Book 2', 'Author B');
    INSERT INTO books VALUES(3, 'Book 3', 'Author A');
  }
} {}

do_test select-books-2.1 {
  execsql {
    SELECT title FROM books WHERE author = 'Author A' ORDER BY id DESC LIMIT 1;
  }
} {Book 3}

finish_test