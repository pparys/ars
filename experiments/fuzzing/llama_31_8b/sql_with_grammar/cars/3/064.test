set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders (
      id INTEGER PRIMARY KEY AUTOINCREMENT,
      customer_name TEXT,
      order_date DATE
    );
    INSERT INTO orders (
      customer_name,
      order_date
    ) VALUES (
      'John Doe',
      '2022-01-01'
    );
    INSERT INTO orders (
      customer_name,
      order_date
    ) VALUES (
      'Jane Doe',
      '2022-01-15'
    );
  }
} {}

do_test select-orders-3.1 {
  execsql {
    SELECT customer_name, order_date
    FROM orders
    WHERE order_date > '2022-01-05'
    ORDER BY order_date ASC
    LIMIT 1;
  }
} {Jane Doe 2022-01-15}

finish_test