set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(id INTEGER PRIMARY KEY, customer_id INTEGER, 
                       total REAL, date DATE);
    INSERT INTO orders(id, customer_id, total, date) 
       VALUES(1, 1, 100.0, '2022-01-01');
    INSERT INTO orders(id, customer_id, total, date) 
       VALUES(2, 2, 200.0, '2022-01-10');
  }
} {}

do_test select-order-2.1 {
  execsql {
    SELECT date, total FROM orders
      WHERE customer_id = 2 ORDER BY date;
  }
} {2022-01-10 200.0}

finish_test