set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-1.0 {
  execsql {
    CREATE TABLE products(id INTEGER PRIMARY KEY, name TEXT, price REAL);
    INSERT INTO products(id, name, price) VALUES(1001, 'IPhone', 999.99);
    INSERT INTO products(id, name, price) VALUES(1002, 'Samsung', 599.99);
  }
} {}

do_test select-product-1.1 {
  execsql {
    SELECT name, price FROM products WHERE price > 750;
  }
} {Samsung 599.99}

finish_test