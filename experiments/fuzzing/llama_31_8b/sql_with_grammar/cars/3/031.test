set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_name TEXT, total REAL);
    INSERT INTO orders VALUES(101, 'John Doe', 100.00);
    INSERT INTO orders VALUES(102, 'Jane Doe', 200.00);
    INSERT INTO orders VALUES(103, 'Alice Smith', 50.00);
  }
} {}

do_test select-expensive-item-3.1 {
  execsql {
    SELECT customer_name FROM orders WHERE total > 100;
  }
} {John Doe Jane Doe}

finish_test