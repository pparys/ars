set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test insert-user-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, product TEXT, customer TEXT);
    INSERT INTO orders(order_id, product, customer) VALUES(301, 'Laptop', 'Alice');
    INSERT INTO orders(order_id, product, customer) VALUES(302, 'Phone', 'Bob');
  }
} {}

do_test select-orders-3.1 {
  execsql {
    SELECT product, customer FROM orders ORDER BY order_id LIMIT 1;
  }
} {Laptop Alice}

finish_test