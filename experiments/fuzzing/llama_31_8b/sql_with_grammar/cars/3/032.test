set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE customers(id INTEGER, name TEXT, city TEXT);
    INSERT INTO customers(id, name, city) VALUES(201, 'John Doe', 'New York');
    INSERT INTO customers(id, name, city) VALUES(202, 'Jane Doe', 'New York');
  }
} {}

do_test select-customer-filter-3.1 {
  execsql {
    SELECT name FROM customers WHERE city = 'New York';
  }
} {John Doe Jane Doe}

finish_test