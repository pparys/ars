set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(customer_id INTEGER, order_date DATE);
    INSERT INTO orders VALUES(1, '2022-01-01');
    INSERT INTO orders VALUES(1, '2022-01-15');
    INSERT INTO orders VALUES(2, '2022-02-01');
  }
} {}

do_test find-oldest-order-2.1 {
  execsql {
    SELECT order_date 
    FROM orders 
    WHERE customer_id = 1 
    ORDER BY order_date 
    LIMIT 1;
  }
} {2022-01-01}

finish_test