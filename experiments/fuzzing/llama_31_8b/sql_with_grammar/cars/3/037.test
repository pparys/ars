set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE products(id INTEGER, name TEXT, price REAL);
    INSERT INTO products(id, name, price) VALUES(201, 'Apple Watch', 299.99);
    INSERT INTO products(id, name, price) VALUES(202, 'Samsung TV', 1299.99);
    INSERT INTO products(id, name, price) VALUES(203, 'Nikon Camera', 899.99);
  }
} {}

do_test select-cheapest-product-2.1 {
  execsql {
    SELECT name, price FROM products ORDER BY price ASC LIMIT 1;
  }
} {Apple Watch 299.99}

finish_test