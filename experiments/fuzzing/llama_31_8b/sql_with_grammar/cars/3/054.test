set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-group-1.0 {
  execsql {
    CREATE TABLE students(id INTEGER, name TEXT, grade INTEGER);
    INSERT INTO students(id, name, grade) VALUES(101, 'Alice', 90);
    INSERT INTO students(id, name, grade) VALUES(102, 'Bob', 85);
  }
} {}

do_test select-student-1.1 {
  execsql {
    SELECT name, grade FROM students ORDER BY grade DESC LIMIT 1;
  }
} {Alice 90}

finish_test