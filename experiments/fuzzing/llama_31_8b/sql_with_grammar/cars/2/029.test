set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test groupby_NULL-1.0 {
  execsql {
    CREATE TABLE t1(a INTEGER, b INTEGER);
    INSERT INTO t1 VALUES(1, NULL);
