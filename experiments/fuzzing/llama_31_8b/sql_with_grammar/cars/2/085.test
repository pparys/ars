set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, product TEXT, quantity INTEGER);
    INSERT INTO orders(id, product, quantity) VALUES(201, 'Product A', 10);
    INSERT INTO orders(id, product, quantity) VALUES(202, 'Product B', 20);
  }
} {}

do_test select-orders-2.1 {
  execsql {
    SELECT product, quantity FROM orders WHERE quantity > 15;
  }
} {Product B 20}

finish_test