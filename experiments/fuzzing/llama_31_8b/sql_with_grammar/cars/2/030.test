set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE employees(id INTEGER, name TEXT, department TEXT);
    INSERT INTO employees(id, name, department) VALUES(201, 'John', 'Sales');
    INSERT INTO employees(id, name, department) VALUES(202, 'Jane', 'Sales');
  }
} {}

do_test select-employee-2.1 {
  execsql {
    SELECT name, department FROM employees WHERE department = 'Sales';
  }
} {John Sales Jane Sales}

finish_test