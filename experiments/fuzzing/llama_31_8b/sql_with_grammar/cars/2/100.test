set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-users-1.0 {
  execsql {
    CREATE TABLE users(id INTEGER PRIMARY KEY, name TEXT, birthday DATE);
    INSERT INTO users(id, name, birthday) VALUES(101, 'Alice', '1990-01-01');
    INSERT INTO users(id, name, birthday) VALUES(102, 'Bob', '1991-02-02');
    INSERT INTO users(id, name, birthday) VALUES(103, 'Charlie', '1992-03-03');
  }
} {}

do_test select-user-order-1.1 {
  execsql {
    SELECT name FROM users ORDER BY birthday ASC;
  }
} {Charlie Alice Bob}

finish_test