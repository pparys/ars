set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-query-3.1 {
  execsql {
    CREATE TABLE scores(team TEXT, year INTEGER, score INTEGER);
    INSERT INTO scores VALUES('Red', 2001, 10);
    INSERT INTO scores VALUES('Red', 2002, 20);
    INSERT INTO scores VALUES('Blue', 2001, 15);
  }
} {}

do_test select-red-scores-3.2 {
  execsql {
    SELECT score FROM scores WHERE team = 'Red' AND year > 2000;
  }
} {20}

finish_test