set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER PRIMARY KEY, customer_name TEXT);
    INSERT INTO orders(order_id, customer_name) VALUES(201, 'John Doe');
    INSERT INTO orders(order_id, customer_name) VALUES(202, 'Jane Doe');
    INSERT INTO orders(order_id, customer_name) VALUES(203, 'Bob Smith');
  }
} {}

do_test select-order-2.1 {
  execsql {
    SELECT customer_name FROM orders ORDER BY order_id DESC LIMIT 1;
  }
} {Bob Smith}

finish_test