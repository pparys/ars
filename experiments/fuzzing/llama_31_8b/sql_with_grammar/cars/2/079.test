set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-multi-column-1.0 {
  execsql {
    CREATE TABLE users(id INTEGER, name TEXT, age INTEGER);
    INSERT INTO users(id, name, age) VALUES(101, 'Alice', 25);
    INSERT INTO users(id, name, age) VALUES(102, 'Bob', 30);
    INSERT INTO users(id, name, age) VALUES(103, 'Charlie', 35);
  }
} {}

do_test select-user-filter-1.1 {
  execsql {
    SELECT name FROM users WHERE age > 30;
  }
} {Charlie}

finish_test