set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-types-1.0 {
  execsql {
    CREATE TABLE books(id INTEGER PRIMARY KEY, 
                       name TEXT, description TEXT, price REAL, qty INTEGER);
    INSERT INTO books VALUES(1, 'Book1', 'Description1', 9.99, 10);
    INSERT INTO books VALUES(2, 'Book2', 'Description2', 19.99, 5);
  }
} {}

do_test select-book-1.1 {
  execsql {
    SELECT name, price FROM books WHERE qty > 5 ORDER BY price DESC LIMIT 1;
  }
} {Book1 9.99}

finish_test