set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, amount REAL, customer_id INTEGER);
    INSERT INTO orders(id, amount, customer_id) VALUES(1, 10.99, 1);
    INSERT INTO orders(id, amount, customer_id) VALUES(2, 20.99, 1);
