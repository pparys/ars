set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, customer_name TEXT);
    INSERT INTO orders(id, customer_name) VALUES(301, 'John Doe');
    INSERT INTO orders(id, customer_name) VALUES(302, 'Jane Doe');
  }
} {}

do_test select-order-3.1 {
  execsql {
    SELECT customer_name 
    FROM orders 
    ORDER BY id ASC 
    LIMIT 1;
  }
} {Jane Doe}

finish_test