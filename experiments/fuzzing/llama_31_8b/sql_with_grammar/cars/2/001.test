set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(id INTEGER Primary Key, order_date DATE DEFAULT CURRENT_DATE, customer_id INTEGER);
    INSERT INTO orders(id, customer_id) VALUES(1001, 123);
    INSERT INTO orders(id, customer_id, order_date) VALUES(1002, 123, '2022-01-01');
  }
} {}

do_test select-order-3.1 {
  execsql {
    SELECT id, order_date FROM orders WHERE customer_id = 123 ORDER BY order_date DESC LIMIT 1;
  }
} {1002 2022-01-01}

finish_test