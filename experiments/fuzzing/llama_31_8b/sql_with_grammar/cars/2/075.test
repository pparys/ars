set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE students(id INTEGER, name TEXT, grade TEXT);
    INSERT INTO students(id, name, grade) VALUES(201, 'John', 'A');
    INSERT INTO students(id, name, grade) VALUES(202, 'Jane', 'B');
  }
} {}

do_test select-student-3.1 {
  execsql {
    SELECT name, grade FROM students WHERE id = 201 ORDER BY grade DESC LIMIT 1;
  }
} {John A}

finish_test