set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_name TEXT);
    INSERT INTO orders(order_id, customer_name) VALUES(301, 'John Doe');
    INSERT INTO orders(order_id, customer_name) VALUES(302, 'Jane Doe');
  }
} {}

do_test select-order-3.1 {
  execsql {
    SELECT customer_name 
    FROM orders 
    WHERE order_id = 302 
    ORDER BY customer_name ASC;
  }
} {Jane Doe}

finish_test