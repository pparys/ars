set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE products(product_id INTEGER PRIMARY KEY, name TEXT, price REAL);
    INSERT INTO products VALUES(1, 'Product A', 19.99);
    INSERT INTO products VALUES(2, 'Product B', 9.99);
    INSERT INTO products VALUES(3, 'Product C', 29.99);
  }
} {}

do_test select-products-3.1 {
  execsql {
    SELECT * FROM products WHERE price > 20 ORDER BY price ASC LIMIT 2;
  }
} {2 Product B 9.99 1 Product A 19.99}

finish_test