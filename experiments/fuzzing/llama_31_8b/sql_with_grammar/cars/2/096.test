set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test order-by-ascending-1.0 {
  execsql {
    CREATE TABLE scores(name TEXT, score REAL);
    INSERT INTO scores(name, score) VALUES('John', 90);
    INSERT INTO scores(name, score) VALUES('Emma', 95);
    INSERT INTO scores(name, score) VALUES('David', 80);
  }
} {}

do_test order-by-ascending-1.1 {
  execsql {
    SELECT name, score FROM scores ORDER BY score ASC;
  }
} {Emma David John}

finish_test