set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE books(id INTEGER, title TEXT, author TEXT);
    INSERT INTO books(id, title, author) VALUES(201, 'Book1', 'Author1');
    INSERT INTO books(id, title, author) VALUES(202, 'Book2', 'Author2');
  }
} {}

do_test select-book-3.1 {
  execsql {
    SELECT title, author FROM books WHERE id = 202;
  }
} {Book2 Author2}

finish_test