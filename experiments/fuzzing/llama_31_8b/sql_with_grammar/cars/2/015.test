set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-1.0 {
  execsql {
    CREATE TABLE books(
      id INTEGER PRIMARY KEY,
      title TEXT NOT NULL,
      author TEXT,
      publication_date TEXT
    );
    INSERT INTO books VALUES(1, 'Book 1', 'Author 1', '2020-01-01');
    INSERT INTO books VALUES(2, 'Book 2', 'Author 2', '2020-01-15');
  }
} {}

do_test select-orderby-1.1 {
  execsql {
    SELECT * FROM books ORDER BY publication_date DESC LIMIT 1;
  }
} {2 Book 2 Author 2 2020-01-15}

finish_test