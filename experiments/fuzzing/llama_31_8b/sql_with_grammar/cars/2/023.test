set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-groups-1.0 {
  execsql {
    CREATE TABLE groups(id INTEGER, name TEXT);
    INSERT INTO groups(id, name) VALUES(1, 'Group A');
    INSERT INTO groups(id, name) VALUES(2, 'Group B');
    INSERT INTO groups(id, name) VALUES(3, 'Group C');
  }
} {}

do_test select-group-by-1.1 {
  execsql {
    SELECT name FROM groups ORDER BY id DESC LIMIT 2;
  }
} {Group C Group B}

finish_test