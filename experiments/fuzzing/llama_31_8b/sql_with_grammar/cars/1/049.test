set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test insert-data-3.0 {
  execsql {
    CREATE TABLE books(title TEXT, author TEXT);
    INSERT INTO books VALUES('The Great Gatsby', 'F. Scott Fitzgerald');
    INSERT INTO books VALUES('To Kill a Mockingbird', 'Harper Lee');
  }
} {}

do_test select-find-3.1 {
  execsql {
    SELECT title FROM books WHERE author = 'F. Scott Fitzgerald';
  }
} {The Great Gatsby}

finish_test