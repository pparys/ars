set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-1.0 {
  execsql {
    CREATE TABLE products(
      id INTEGER PRIMARY KEY,
      name TEXT NOT NULL,
      price REAL 
    );
    INSERT INTO products(name, price) VALUES('Laptop', 1000);
    INSERT INTO products(name, price) VALUES('Tablet', 800);
  }
} {}

do_test select-product-1.1 {
  execsql {
    SELECT price FROM products 
    ORDER BY price DESC 
    LIMIT 1;
  }
} {1000}

finish_test