set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer TEXT, order_date DATE);
    INSERT INTO orders VALUES(101, 'John Doe', '2022-01-01');
    INSERT INTO orders VALUES(102, 'Jane Doe', '2022-01-15');
  }
} {}

do_test select-order-3.1 {
  execsql {
    SELECT order_date FROM orders WHERE customer = 'John Doe';
  }
} {2022-01-01}

finish_test