set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-extra-dates-1.0 {
  execsql {
    CREATE TABLE calendars(year INTEGER, month INTEGER, day INTEGER);
    INSERT INTO calendars VALUES(2022, 1, 1);
    INSERT INTO calendars VALUES(2022, 1, 2);
    INSERT INTO calendars VALUES(2022, 1, 3);
  }
} {}

do_test select-different-month-1.1 {
  execsql {
    SELECT * FROM calendars WHERE month = 1;
  }
} {2022 1 1 2022 1 2 2022 1 3}

finish_test