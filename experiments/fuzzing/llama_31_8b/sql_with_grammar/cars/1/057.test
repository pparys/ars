do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, customer_id INTEGER, order_date DATE, total REAL);
    INSERT INTO orders(id, customer_id, order_date, total) VALUES(201, 1, '2020-01-01', 100.0);
    INSERT INTO orders(id, customer_id, order_date, total) VALUES(202, 1, '2021-01-01', 200.0);
    INSERT INTO orders(id, customer_id, order_date, total) VALUES(203, 2, '2020-01-01', 50.0);
  }
} {}

do_test select-orders-2.1 {
