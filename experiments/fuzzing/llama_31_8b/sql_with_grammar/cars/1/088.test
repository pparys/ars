set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-orderby-1.0 {
  execsql {
    CREATE TABLE scores(name TEXT, score REAL);
    INSERT INTO scores VALUES('John', 90.5);
    INSERT INTO scores VALUES('Alice', 85.2);
    INSERT INTO scores VALUES('Bob', 98.7);
  }
} {}

do_test select-score-from-names-1.1 {
  execsql {
    SELECT name, score FROM scores ORDER BY score ASC;
  }
} {John 90.5 Alice 85.2 Bob 98.7}

finish_test