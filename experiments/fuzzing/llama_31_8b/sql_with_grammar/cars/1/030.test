set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE books(id INTEGER, title TEXT, author TEXT);
    INSERT INTO books(id, title, author) VALUES(201, 'Book 1', 'Author 1');
    INSERT INTO books(id, title, author) VALUES(202, 'Book 2', 'Author 2');
  }
} {}

do_test query-order-3.1 {
  execsql {
    SELECT title, author FROM books ORDER BY title LIMIT 1;
  }
} {Book 1 Author 1}

finish_test