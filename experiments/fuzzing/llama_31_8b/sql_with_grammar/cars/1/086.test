set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-order-2.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_id INTEGER, order_date TEXT);
    INSERT INTO orders(order_id, customer_id, order_date) VALUES(1, 101, '2022-01-01');
    INSERT INTO orders(order_id, customer_id, order_date) VALUES(2, 102, '2022-01-15');
