set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE products(code TEXT PRIMARY KEY, name TEXT, price REAL);
    INSERT INTO products VALUES('A', 'Apple', 1.99);
    INSERT INTO products VALUES('B', 'Banana', 0.99);
  }
} {}

do_test select-product-2.1 {
  execsql {
    SELECT name, price FROM products WHERE price > 1.0;
  }
} {Apple 1.99}

finish_test