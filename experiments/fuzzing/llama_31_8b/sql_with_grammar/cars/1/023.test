  execsql {
    CREATE TABLE orders(id INTEGER, product TEXT, customer TEXT);
    INSERT INTO orders(id, product, customer) VALUES(201, 'Laptop', 'Charlie');
    INSERT INTO orders(id, product, customer) VALUES(202, 'Phone', 'David');
