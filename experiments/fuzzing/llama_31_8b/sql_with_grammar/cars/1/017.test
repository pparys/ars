set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-sort-1.0 {
  execsql {
    CREATE TABLE numbers(id INTEGER, num REAL);
    INSERT INTO numbers VALUES(1, 10.5);
    INSERT INTO numbers VALUES(2, 5.7);
    INSERT INTO numbers VALUES(3, 8.9);
  }
} {}

do_test select-sort-1.1 {
  execsql {
    SELECT num FROM numbers ORDER BY num ASC;
  }
} {5.7 8.9 10.5}

finish_test