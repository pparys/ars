set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE customers(id INTEGER, name TEXT, purchase UINT);
    INSERT INTO customers VALUES(1, 'Jhon', 200);
    INSERT INTO customers VALUES(2, 'Doe', 100);
    INSERT INTO customers VALUES(3, 'Smith', 300);
  }
} {}

