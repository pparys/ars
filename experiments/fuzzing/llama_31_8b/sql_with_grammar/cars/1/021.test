set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-user-2.0 {
  execsql {
    CREATE TABLE users2(id INTEGER PRIMARY KEY AUTOINCREMENT, name TEXT UNIQUE);
    INSERT INTO users2 VALUES(NULL, 'Charlie');
    INSERT INTO users2 VALUES(5, 'Dave');
  }
} {}

do_test select-users-2.1 {
  execsql {
    SELECT name FROM users2 WHERE id > 2;
  }
} {Charlie}

finish_test