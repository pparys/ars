set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-tags-3.0 {
  execsql {
    CREATE TABLE tags(name TEXT PRIMARY KEY);
    INSERT INTO tags VALUES('friend');
    INSERT INTO tags VALUES('family');
  }
} {}

