set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.1 {
  execsql {
    CREATE TABLE students(id INTEGER, name TEXT, age INTEGER);
    INSERT INTO students(id, name, age) VALUES(201, 'John Doe', 20);
    INSERT INTO students(id, name, age) VALUES(202, 'Jane Doe', 22);
  }
} {}

do_test select-older-3.2 {
  execsql {
    SELECT name FROM students WHERE age > 21;
  }
} {Jane Doe}

finish_test