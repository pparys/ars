set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER PRIMARY KEY, customer_name TEXT, order_date DATE);
    INSERT INTO orders(order_id, customer_name, order_date) VALUES(101, 'John Doe', '2022-01-01');
    INSERT INTO orders(order_id, customer_name, order_date) VALUES(102, 'Jane Doe', '2022-01-15');
  }
} {}

do_test select-orders-2.1 {
  execsql {
    SELECT customer_name, order_date FROM orders ORDER BY order_date ASC;
  }
} {Jane Doe 2022-01-15 John Doe 2022-01-01}

finish_test