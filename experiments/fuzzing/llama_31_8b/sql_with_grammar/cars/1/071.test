set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-4.0 {
  execsql {
    CREATE TABLE products(product_id INTEGER PRIMARY KEY, name TEXT, price REAL);
    INSERT INTO products(product_id, name, price) VALUES(1001, 'Laptop', 1200.99);
    INSERT INTO products(product_id, name, price) VALUES(1002, 'Phone', 500.99);
  }
} {}

