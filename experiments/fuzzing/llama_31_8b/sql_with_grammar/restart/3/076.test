set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-users-3.0 {
  execsql {
    CREATE TABLE users(id INTEGER PRIMARY KEY, name TEXT);
    INSERT INTO users(id, name) VALUES(1000, 'Alice');
    INSERT INTO users(id, name) VALUES(1001, 'Bob');
  }
} {}

do_test delete-user-3.1 {
  execsql {
    DELETE FROM users WHERE name = 'Alice' AND id = 1000;
  }
} {}

finish_test