set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-1.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_id INTEGER, order_date DATE, TOTAL REAL);
    INSERT INTO orders VALUES(1, 101, '2022-01-01', 10.99);
