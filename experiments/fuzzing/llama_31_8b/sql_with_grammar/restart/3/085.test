set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-sqlite-data-1.0 {
  execsql {
    CREATE TABLE customers(id INTEGER PRIMARY KEY, name TEXT);
    INSERT INTO customers(id, name) VALUES(100, 'John');
    INSERT INTO customers(id, name) VALUES(101, 'Julia');
    INSERT INTO customers(id, name) VALUES(102, 'Jane');
  }
} {}

