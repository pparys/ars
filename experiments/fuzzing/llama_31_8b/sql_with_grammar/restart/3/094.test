set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-multi-column-index-1.0 {
  execsql {
    CREATE TABLE data(a INTEGER, b INTEGER, c INTEGER);
    INSERT INTO data VALUES(1, 2, 3);
    INSERT INTO data VALUES(4, 5, 6);
    INSERT INTO data VALUES(7, 8, 9);
    INSERT INTO data VALUES(10, 11, 12);
    INSERT INTO data VALUES(13, 14, 15);
  }
} {}

do_test query-multi-column-index-1.1 {
  execsql {
    SELECT MINized_col, SUMed_col
    FROM
    data 
    ORDER BY 
    a 
    ASC,
    b 
    DESC
  ;}
} {1 33}

finish_test