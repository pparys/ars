set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-primary-1.0 {
  execsql {
    CREATE TABLE employees(id INTEGER PRIMARY KEY, name TEXT, salary REAL);
    INSERT INTO employees VALUES(1001, 'John', 50000.0);
    INSERT INTO employees VALUES(1002, 'Jane', 60000.0);
  }
} {}

do_test query-employees-1.1 {
  execsql {
    SELECT name, salary FROM employees ORDER BY salary ASC;
  }
} {Jane 60000.0 John 50000.0}

finish_test