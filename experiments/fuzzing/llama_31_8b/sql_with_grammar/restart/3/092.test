set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-order-1.0 {
  execsql {
    CREATE TABLE scores (
      id INTEGER PRIMARY KEY,
      student_name TEXT,
      score INTEGER
    );
    INSERT INTO scores (student_name, score) 
      VALUES ('John', 80);
    INSERT INTO scores (student_name, score) 
      VALUES ('Jane', 90);
  }
} {}

do_test select-score-order-1.1 {
  execsql {
    SELECT student_name, score FROM scores ORDER BY score DESC;
  }
} { Jane 90 John 80 }

finish_test