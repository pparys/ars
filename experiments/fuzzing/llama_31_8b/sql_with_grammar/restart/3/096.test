set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-1.0 {
  execsql {
    CREATE TABLE company(
      id INTEGER PRIMARY KEY,
      name TEXT NOT NULL,
      city TEXT
    );
    INSERT INTO company(id, name, city) VALUES(1, 'ABC Inc', 'New York');
    INSERT INTO company(id, name, city) VALUES(2, 'YZC Ltd', 'Los Angeles');
  }
} {}

do_test delete-from-view-1.1 {
  execsql {
    SELECT * FROM company;
  }
} {1 ABC Inc New York 2 YZC Ltd Los Angeles}

finish_test