set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE employees(
      id INTEGER,
      name TEXT,
      salary REAL,
      department_id INTEGER
    );
    INSERT INTO employees(id, name, salary, department_id) 
    VALUES(1, 'John', 50000, 10);
    INSERT INTO employees(id, name, salary, department_id) 
    VALUES(2, 'Alice', 60000, 20);
    INSERT INTO employees(id, name, salary, department_id) 
    VALUES(3, 'Bob', 70000, 30);
  }
} {}

do_test group-by-3.1 {
  execsql {
    SELECT department_id, name, MAXsalary 
    FROM employees 
    GROUP BY department_id 
    ORDER BY department_id;
  }
} {{10 Bob}  


     	

       
     

        

  
          

         


