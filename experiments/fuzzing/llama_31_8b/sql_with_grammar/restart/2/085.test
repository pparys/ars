set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-priority-2.0 {
  execsql {
    CREATE TABLE categories(id INTEGER, priority INTEGER);
    INSERT INTO categories VALUES(1, 5);
    INSERT INTO categories VALUES(2, 1);
    INSERT INTO categories VALUES(3, 3);
  }
} {}

do_test priority-sorts-2.1 {
  execsql {
    SELECT id, priority FROM categories ORDER BY priority DESC;
  }
} {1 5 3 3 2 1}

finish_test