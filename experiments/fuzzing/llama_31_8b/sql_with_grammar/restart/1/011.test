set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE products(product_id INTEGER PRIMARY KEY, product_name TEXT, price REAL);
    INSERT INTO products(product_id, product_name, price) VALUES(1, 'Product A', 19.99);
    INSERT INTO products(product_id, product_name, price) VALUES(2, 'Product B', 9.99);
  }
} {}

do_test select-product-3.1 {
  execsql {
    SELECT product_name, price FROM products WHERE price > 15;
  }
} {Product A 19.99}

finish_test