set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(
      id INTEGER PRIMARY KEY,
      customer_id INTEGER,
      order_date DATE
    );
    INSERT INTO orders VALUES(1, 1, '2022-01-01');
    INSERT INTO orders VALUES(2, 1, '2022-01-15');
  }
} {}

do_test query-orders-3.1 {
  execsql {
    SELECT order_date
    FROM orders
    WHERE customer_id = 1
    ORDER BY order_date DESC
    LIMIT 1;
  }
} {2022-01-15}

finish_test