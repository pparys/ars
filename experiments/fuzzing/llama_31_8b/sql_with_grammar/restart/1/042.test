set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE addresses(id INTEGER, street TEXT, city TEXT);
    INSERT INTO addresses(id, street, city) VALUES(201, '123 Main St', 'Anytown');
    INSERT INTO addresses(id, street, city) VALUES(202, '456 Elm St', 'Othertown');
  }
} {}

do_test select-address-3.1 {
  execsql {
    SELECT street, city FROM addresses WHERE id = 202;
  }
} {456 Elm St Othertown}

finish_test