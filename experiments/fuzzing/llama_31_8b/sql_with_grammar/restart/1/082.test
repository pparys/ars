set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_name TEXT, total REAL);
    INSERT INTO orders(order_id, customer_name, total) VALUES(1, 'John Doe', 100.0);
    INSERT INTO orders(order_id, customer_name, total) VALUES(2, 'Jane Doe', 200.0);
  }
} {}

do_test select-order-3.1 {
  execsql {
    SELECT customer_name, total FROM orders WHERE order_id = 1;
  }
} {John Doe 100.0}

finish_test