set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-sort-order-1.0 {
  execsql {
    CREATE TABLE scores(name TEXT, score INTEGER);
    INSERT INTO scores VALUES('Alice', 90);
    INSERT INTO scores VALUES('Bob', 95);
  }
} {}

