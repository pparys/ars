set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE books(id INTEGER, title TEXT, author TEXT);
    INSERT INTO books(id, title, author) VALUES(201, 'To Kill a Mockingbird', 'Harper Lee');
    INSERT INTO books(id, title, author) VALUES(202, '1984', 'George Orwell');
    INSERT INTO books(id, title, author) VALUES(203, 'Pride and Prejudice', 'Jane Austen');
  }
} {}

do_test query-books-2.1 {
  execsql {
    SELECT title, author FROM books ORDER BY author;
  }
} {1984 George Orwell Pride and Prejudice Jane Austen To Kill a Mockingbird Harper Lee}

finish_test