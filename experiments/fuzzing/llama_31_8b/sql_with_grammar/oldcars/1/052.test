set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_name TEXT);
    INSERT INTO orders(order_id, customer_name) VALUES(301, 'Customer 301');
    INSERT INTO orders(order_id, customer_name) VALUES(302, 'Customer 302');
  }
} {}

