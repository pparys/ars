set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, cusName TEXT, amount REAL);
    INSERT INTO orders VALUES(101, 'John Doe', 100.0);
    INSERT INTO orders VALUES(102, 'Jane Doe', 200.0);
    INSERT INTO orders VALUES(103, 'Bob Smith', 300.0);
  }
} {}

do_test select-user-2.1 {
  execsql {
    SELECT cusName FROM orders WHERE amount > 150.0;
  }
} {Jane Doe Bob Smith}

finish_test