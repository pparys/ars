    INSERT INTO orders VALUES(1001, 'John Doe', '2022-01-01');
    INSERT INTO orders VALUES(1002, 'Jane Doe', '2022-02-01');
    INSERT INTO orders VALUES(1003, 'Bob Smith', '2022-03-01');
