set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-1.0 {
  execsql {
    CREATE TABLE products(id INTEGER, price REAL);
    INSERT INTO products(id, price) VALUES(1, 9.99);
    INSERT INTO products(id, price) VALUES(2, 19.99);
  }
} {}

do_test select-product-1.1 {
  execsql {
    SELECT id, price FROM products WHERE price > 10;
  }
} {2 19.99}

finish_test