set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE books(id INTEGER PRIMARY KEY, title TEXT, author TEXT);
    INSERT INTO books VALUES(1, 'To Kill a Mockingbird', 'Harper Lee');
    INSERT INTO books VALUES(2, '1984', 'George Orwell');
    INSERT INTO books VALUES(3, 'Pride and Prejudice', 'Jane Austen');
  }
} {}

do_test fetch-sort-2.1 {
  execsql {
    SELECT title, author FROM books ORDER BY author;
  }
} {Pride and Prejudice Jane Austen 1984 George Orwell To Kill a Mockingbird Harper Lee}

finish_test