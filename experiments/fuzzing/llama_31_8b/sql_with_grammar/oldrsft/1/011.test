set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE employees(
      id INTEGER PRIMARY KEY,
      name TEXT NOT NULL,
      salary REAL NOT NULL
    );
    INSERT INTO employees VALUES(1, 'John Doe', 50000.0);
    INSERT INTO employees VALUES(2, 'Jane Doe', 60000.0);
  }
} {}

do_test select-salary-2.1 {
  execsql {
    SELECT name, salary FROM employees WHERE salary > 55000;
  }
} {Jane Doe 60000.0}

finish_test