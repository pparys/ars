set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE products(product_id INTEGER, name TEXT, price REAL);
    INSERT INTO products(product_id, name, price) VALUES(1001, 'Product 1', 10.99);
    INSERT INTO products(product_id, name, price) VALUES(1002, 'Product 2', 20.99);
    INSERT INTO products(product_id, name, price) VALUES(1003, 'Product 3', 30.99);
  }
} {}

do_test select-product-3.1 {
  execsql {
    SELECT name FROM products WHERE price > 20;
  }
} {Product 3}

finish_test