set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE products(
      id INTEGER,
      name TEXT,
      category TEXT,
      price REAL
    );
    INSERT INTO products(id, name, category, price) VALUES(
      1001, 'Product A', 'Category X', 9.99
    );
    INSERT INTO products(id, name, category, price) VALUES(
      1002, 'Product B', 'Category Y', 19.99
    );
    INSERT INTO products(id, name, category, price) VALUES(
      1003, 'Product C', 'Category Z', 29.99
    );
  }
} {}

do_test select-expensive-3.1 {
  execsql {
    SELECT name FROM products WHERE price > 20;
  }
} {Product C}

finish_test