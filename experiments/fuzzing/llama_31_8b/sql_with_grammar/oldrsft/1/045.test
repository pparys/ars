set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE employees(id INTEGER PRIMARY KEY, name TEXT, salary REAL);
    INSERT INTO employees(id, name, salary) VALUES(1, 'John Doe', 50000.0);
    INSERT INTO employees(id, name, salary) VALUES(2, 'Jane Doe', 60000.0);
  }
} {}

do_test query-employees-2.1 {
  execsql {
    SELECT name, salary FROM employees ORDER BY salary DESC LIMIT 1;
  }
} {Jane Doe 60000.0}

finish_test