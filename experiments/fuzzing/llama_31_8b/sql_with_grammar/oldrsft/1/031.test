set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER PRIMARY KEY, customer TEXT NOT NULL, date TEXT);
    INSERT INTO orders VALUES(101, 'Alice', '2022-01-01');
    INSERT INTO orders VALUES(102, 'Bob', '2022-01-02');
  }
} {}

do_test delete-order-3.1 {
  execsql {
    DELETE FROM orders WHERE customer = 'Alice';
  }
} {}

finish_test