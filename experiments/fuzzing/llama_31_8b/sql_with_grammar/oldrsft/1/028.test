set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE books(id INTEGER, title TEXT, author TEXT, price REAL);
    INSERT INTO books VALUES(1, 'Book1', 'Author1', 12.99);
    INSERT INTO books VALUES(2, 'Book2', 'Author2', 9.99);
    INSERT INTO books VALUES(3, 'Book3', 'Author3', 5.99);
  }
} {}

do_test select-top-3-2.1 {
  execsql {
    SELECT title, price FROM books ORDER BY price DESC LIMIT 3;
  }
} {Book1 12.99 Book2 9.99 Book3 5.99}

finish_test