set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_id INTEGER, total REAL);
    INSERT INTO orders(order_id, customer_id, total) VALUES(1, 1, 100.5);
    INSERT INTO orders(order_id, customer_id, total) VALUES(2, 2, 200.5);
  }
} {}

do_test select-order-2.1 {
  execsql {
    SELECT customer_id, total FROM orders ORDER BY total DESC LIMIT 1;
  }
} {2 200.5}

finish_test