set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE books(isbn INTEGER, title TEXT, author TEXT);
    INSERT INTO books VALUES(1, 'To Kill a Mockingbird', 'Harper Lee');
    INSERT INTO books VALUES(2, '1984', 'George Orwell');
    INSERT INTO books VALUES(3, 'The Great Gatsby', 'F. Scott Fitzgerald');
  }
} {}

do_test select-books-2.1 {
  execsql {
    SELECT title, author FROM books WHERE isbn = 2;
  }
} {1984 George Orwell}

finish_test