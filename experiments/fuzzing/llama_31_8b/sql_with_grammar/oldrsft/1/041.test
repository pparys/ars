set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE students(id INTEGER, name TEXT, age INTEGER);
    INSERT INTO students(id, name, age) VALUES(201, 'David', 20);
    INSERT INTO students(id, name, age) VALUES(202, 'Emily', 21);
  }
} {}

do_test select-older-student-2.1 {
  execsql {
    SELECT name, age FROM students WHERE age > 20;
  }
} {David 20}

finish_test