set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_name TEXT, total REAL);
    INSERT INTO orders(order_id, customer_name, total) VALUES(201, 'Alice', 99.99);
    INSERT INTO orders(order_id, customer_name, total) VALUES(202, 'Bob', 199.99);
  }
} {}

do_test update-order-2.1 {
  execsql {
    UPDATE orders SET total = 299.99 WHERE customer_name = 'Alice';
    SELECT total FROM orders WHERE customer_name = 'Alice';
  }
} {299.99}

finish_test