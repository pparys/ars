set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE employees(id INTEGER, name TEXT, salary REAL);
    INSERT INTO employees(id, name, salary) VALUES(201, 'John', 50000.0);
    INSERT INTO employees(id, name, salary) VALUES(202, 'Jane', 60000.0);
  }
} {}

do_test select-employees-2.1 {
  execsql {
    SELECT name, salary FROM employees ORDER BY salary DESC LIMIT 1;
  }
} {Jane 60000.0}

finish_test