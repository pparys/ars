set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data3-1.0 {
  execsql {
    CREATE TABLE orders(
        id INTEGER PRIMARY KEY,
        customer_name TEXT NOT NULL,
        order_date DATE NOT NULL
    );
    INSERT INTO orders(customer_name, order_date) VALUES('Customer1', '2022-01-01');
    INSERT INTO orders(customer_name, order_date) VALUES('Customer2', '2022-01-15');
