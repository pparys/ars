set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE products(id INTEGER, name TEXT, price REAL);
    INSERT INTO products VALUES(201, 'Product A', 10.99);
    INSERT INTO products VALUES(202, 'Product B', 20.99);
    INSERT INTO products VALUES(203, 'Product C', 5.99);
  }
} {}

do_test query-by-price-3.1 {
  execsql {
    SELECT name, price FROM products ORDER BY price DESC LIMIT 1;
  }
} {Product B 20.99}

finish_test