set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, product TEXT, price REAL);
    INSERT INTO orders VALUES(1, 'Product A', 19.99);
    INSERT INTO orders VALUES(2, 'Product B', 9.99);
  }
} {}

do_test select-cheapest-3.1 {
  execsql {
    SELECT product, price FROM orders ORDER BY price ASC LIMIT 1;
  }
} {Product B 9.99}

finish_test