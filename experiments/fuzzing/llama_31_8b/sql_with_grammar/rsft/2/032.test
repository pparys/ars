set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, customer_name TEXT, order_date DATE);
    INSERT INTO orders VALUES(101, 'John Doe', '2022-01-01');
    INSERT INTO orders VALUES(102, 'Jane Doe', '2022-01-15');
  }
} {}

