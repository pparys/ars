set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, customer_name TEXT, order_date DATE);
    INSERT INTO orders(id, customer_name, order_date) VALUES(1001, 'John Doe', '2015-01-01');
    INSERT INTO orders(id, customer_name, order_date) VALUES(1002, 'Jane Doe', '2016-01-01');
  }
} {}

do_test group-by-order-3.1 {
  execsql {
    SELECT customer_name, order_date 
    FROM orders 
    GROUP BY customer_name 
    ORDER BY order_date DESC;
  }
} {Jane Doe 2016-01-01 Jane Doe 2016-01-01 John Doe 2015-01-01 John Doe 2015-01-01}

finish_test