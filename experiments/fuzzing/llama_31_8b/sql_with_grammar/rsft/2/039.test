set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE products (
      id INTEGER PRIMARY KEY,
      name TEXT,
      price REAL
    );
    INSERT INTO products VALUES(101, 'Laptop', 1999.99);
    INSERT INTO products VALUES(102, 'Desktop', 999.99);
  }
} {}

do_test select-product-price-3.1 {
  execsql {
    SELECT price FROM products WHERE name = 'Laptop';
  }
} {1999.99}

finish_test