set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_name TEXT, total REAL);
    INSERT INTO orders(order_id, customer_name, total) VALUES(1001, 'John Doe', 10.99);
    INSERT INTO orders(order_id, customer_name, total) VALUES(1002, 'Jane Doe', 20.99);
  }
} {}

do_test select-order-2.1 {
  execsql {
    SELECT customer_name, total FROM orders WHERE total > 15;
  }
} {Jane Doe 20.99}

finish_test