set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, product TEXT, customer TEXT);
    INSERT INTO orders(order_id, product, customer) VALUES(201, 'Product A', 'John');
    INSERT INTO orders(order_id, product, customer) VALUES(202, 'Product B', 'Jane');
  }
} {}

do_test query-orders-2.1 {
  execsql {
    SELECT product, customer FROM orders WHERE order_id = 202;
  }
} {Product B Jane}

finish_test