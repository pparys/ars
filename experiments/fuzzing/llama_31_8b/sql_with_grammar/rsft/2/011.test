set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE scores(
      student TEXT UNIQUE,
      math INTEGER,
      science INTEGER
    );
    INSERT INTO scores VALUES('Alice', 90, 80);
    INSERT INTO scores VALUES('Bob', 70, 90);
    INSERT INTO scores VALUES('Charlie', 80, 70);
  }
} {}

do_test select-highest-math-3.1 {
  execsql {
    SELECT student FROM scores ORDER BY math DESC LIMIT 1;
  }
} {Bob}

finish_test