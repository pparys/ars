set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER PRIMARY KEY, product_name TEXT, quantity INTEGER);
    INSERT INTO orders VALUES(1, 'Bike', 2);
    INSERT INTO orders VALUES(2, 'Book', 3);
    INSERT INTO orders VALUES(3, 'Phone', 1);
  }
} {}

do_test select-quantity-2.1 {
  execsql {
    SELECT quantity FROM orders WHERE product_name = 'Book';
  }
} {3}

finish_test