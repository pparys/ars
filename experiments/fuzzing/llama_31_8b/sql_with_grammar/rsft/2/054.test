set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_name TEXT, order_date DATE);
    INSERT INTO orders VALUES(301, 'John Doe', '2022-01-01');
    INSERT INTO orders VALUES(302, 'Jane Doe', '2022-01-15');
  }
} {}

do_test select-orders-3.1 {
  execsql {
    SELECT order_id, customer_name FROM orders WHERE order_date = '2022-01-01';
  }
} {301 John Doe}

finish_test