set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER PRIMARY KEY, customer_name TEXT);
    INSERT INTO orders VALUES(101, 'John Smith');
    INSERT INTO orders VALUES(102, 'Jane Doe');
  }
} {}

do_test select-order-2.1 {
  execsql {
    SELECT customer_name FROM orders WHERE order_id = 102;
  }
} {Jane Doe}

finish_test