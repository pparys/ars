set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-store-1.0 {
  execsql {
    CREATE TABLE items(id INTEGER PRIMARY KEY, name TEXT, price REAL);
    INSERT INTO items VALUES(1, 'Apple', 0.99);
    INSERT INTO items VALUES(2, 'Banana', 0.59);
    INSERT INTO items VALUES(3, 'Cherry', 0.79);
  }
} {}

do_test select-most-expensive-1.1 {
  execsql {
    SELECT name FROM items ORDER BY price DESC LIMIT 1;
  }
} {Apple}

finish_test