set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, product TEXT);
    INSERT INTO orders(order_id, product) VALUES(201, 'Book');
    INSERT INTO orders(order_id, product) VALUES(202, 'Pen');
    INSERT INTO orders(order_id, product) VALUES(203, 'Paper');
  }
} {}

