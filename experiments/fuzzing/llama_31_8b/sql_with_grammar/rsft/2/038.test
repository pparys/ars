set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-multiple-1.0 {
  execsql {
    CREATE TABLE products(id INTEGER PRIMARY KEY, name TEXT, price REAL);
    INSERT INTO products(id, name, price) VALUES(1, 'Product A', 10.99);
    INSERT INTO products(id, name, price) VALUES(2, 'Product B', 5.99);
    INSERT INTO products(id, name, price) VALUES(3, 'Product C', 7.99);
  }
} {}

do_test select-products-1.1 {
  execsql {
    SELECT name FROM products WHERE price > 7;
  }
} {Product A}

finish_test