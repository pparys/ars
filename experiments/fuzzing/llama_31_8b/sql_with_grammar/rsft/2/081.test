set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE products(category TEXT, name TEXT, price REAL);
    INSERT INTO products VALUES('Electronics', 'LED TV', 1000.0);
    INSERT INTO products VALUES('Electronics', 'Smartphone', 800.0);
    INSERT INTO products VALUES('Clothing', 'Shirt', 20.0);
  }
} {}

do_test select-expensive-products-2.1 {
  execsql {
    SELECT name FROM products WHERE price > 500.0 AND category = 'Electronics';
  }
} {LED TV}

finish_test