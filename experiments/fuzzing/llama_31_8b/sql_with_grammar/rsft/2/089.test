set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE addresses(id INTEGER, contact_id INTEGER, street TEXT);
    INSERT INTO addresses(id, contact_id, street) VALUES(201, 1, '123 Main St');
    INSERT INTO addresses(id, contact_id, street) VALUES(202, 1, '456 Park Ave');
    INSERT INTO addresses(id, contact_id, street) VALUES(203, 2, '789 Oak St');
  }
} {}

do_test select-address-2.1 {
  execsql {
    SELECT street FROM addresses WHERE contact_id = 1;
  }
} {123 Main St 456 Park Ave}

finish_test