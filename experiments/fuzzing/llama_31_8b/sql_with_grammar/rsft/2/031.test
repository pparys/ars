set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER PRIMARY KEY, customer_name TEXT, order_date DATE);
    INSERT INTO orders(order_id, customer_name, order_date) 
    VALUES(101, 'Alice Brown', '2020-01-01');
    INSERT INTO orders(order_id, customer_name, order_date) 
    VALUES(102, 'Bob Johnson', '2020-01-15');
  }
} {}

do_test select-order-3.1 {
  execsql {
    SELECT customer_name FROM orders WHERE order_id = 102;
  }
} {Bob Johnson}

finish_test