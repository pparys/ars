set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, order_date DATE, status TEXT, customer_id INTEGER);
    INSERT INTO orders(id, order_date, status, customer_id) 
      VALUES(201, '2022-01-01', 'pending', 301);
    INSERT INTO orders(id, order_date, status, customer_id) 
      VALUES(202, '2022-01-15', 'shipped', 302);
  }
} {}

do_test select-shipped-orders-2.1 {
  execsql {
    SELECT order_date FROM orders WHERE status = 'shipped' ORDER BY order_date DESC LIMIT 1;
  }
} {2022-01-15}

finish_test