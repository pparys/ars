set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_name TEXT, total REAL);
    INSERT INTO orders(order_id, customer_name, total) VALUES(101, 'Alice', 100.0);
    INSERT INTO orders(order_id, customer_name, total) VALUES(102, 'Bob', 200.0);
    INSERT INTO orders(order_id, customer_name, total) VALUES(103, 'Charlie', 300.0);
  }
} {}

do_test select-order-3.1 {
  execsql {
    SELECT customer_name FROM orders WHERE total > 150.0 ORDER BY customer_name ASC;
  }
} {Alice Bob Charlie}

finish_test