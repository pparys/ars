set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_id INTEGER, product TEXT, total REAL);
    INSERT INTO orders(order_id, customer_id, product, total) VALUES(301, 111, 'Product A', 100.0);
    INSERT INTO orders(order_id, customer_id, product, total) VALUES(302, 112, 'Product B', 200.0);
  }
} {}

do_test select-order-3.1 {
  execsql {
    SELECT order_id, customer_id, product FROM orders WHERE total > 150.0 AND customer_id = 111;
  }
} {301 111 Product A}

finish_test