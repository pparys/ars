set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-orders-1.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, customer_id INTEGER, total REAL);
    INSERT INTO orders(id, customer_id, total) VALUES(101, 1, 100.00);
    INSERT INTO orders(id, customer_id, total) VALUES(102, 2, 200.00);
  }
} {}

do_test select-order-2.1 {
  execsql {
    SELECT customer_id, total FROM orders ORDER BY total DESC LIMIT 1;
  }
} {2 200.00}

finish_test