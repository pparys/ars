set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE customers(id INTEGER, name TEXT PRIMARY KEY, email TEXT UNIQUE);
    INSERT INTO customers(id, name, email) VALUES(201, 'Ohm', 'ohm@example.com');
    INSERT INTO customers(id, name, email) VALUES(202, 'Ampere', 'ampere@example.com');
  }
} {}

do_test delete-exists-2.1 {
  execsql {
    DELETE FROM customers WHERE email = 'ohm@example.com';
    SELECT name FROM customers WHERE email = 'ohm@example.com';
  }
} {}
finish_test