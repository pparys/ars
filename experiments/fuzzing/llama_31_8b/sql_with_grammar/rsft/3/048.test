set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_name TEXT, total REAL);
    INSERT INTO orders(order_id, customer_name, total) 
      VALUES(101, 'Customer A', 100.0);
    INSERT INTO orders(order_id, customer_name, total) 
      VALUES(102, 'Customer B', 200.0);
  }
} {}

do_test select-orders-over-100-3.1 {
  execsql {
    SELECT customer_name FROM orders WHERE total > 100;
  }
} {Customer B}

finish_test