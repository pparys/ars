set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-geo-1.0 {
  execsql {
    CREATE TABLE points(x INTEGER, y INTEGER);
    INSERT INTO points VALUES(1, 2);
    INSERT INTO points VALUES(3, 4);
  }
} {}

do_test select-geo-1.1 {
  execsql {
    SELECT x, y FROM points WHERE x > 2 ORDER BY x DESC LIMIT 1;
  }
} {3 4}

finish_test