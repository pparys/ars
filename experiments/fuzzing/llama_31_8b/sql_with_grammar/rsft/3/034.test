set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(id INTEGER PRIMARY KEY, customerid INTEGER, total REAL);
    INSERT INTO orders VALUES(1, 1, 100.0);
