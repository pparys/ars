set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE addresses(
      id INTEGER PRIMARY KEY,
      street TEXT,
      number INT,
      city TEXT,
      postal_code INT
    );
    INSERT INTO addresses(id, street, number, city, postal_code) 
    VALUES (1, 'Sunset Boulevard', 123, 'Los Angeles', 90001);
    INSERT INTO addresses(id, street, number, city, postal_code) 
    VALUES (2, 'Beverly Hills Drive', 456, 'Beverly Hills', 90210);
  }
} {}

do_test select-address-2.1 {
  execsql {
    SELECT city, postal_code FROM addresses WHERE id = 1;
  }
} {Los Angeles 90001}

finish_test