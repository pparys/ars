set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_name TEXT, order_date DATE);
    INSERT INTO orders(order_id, customer_name, order_date) VALUES(1001, 'John Doe', '2022-01-01');
    INSERT INTO orders(order_id, customer_name, order_date) VALUES(1002, 'Jane Doe', '2022-01-15');
  }
} {}

do_test select-order-3.1 {
  execsql {
    SELECT customer_name, order_date 
    FROM orders 
    WHERE order_date >= '2022-01-01' AND order_date < '2022-01-20' 
    ORDER BY customer_name ASC;
  }
} {Jane Doe 2022-01-15 John Doe 2022-01-01}

finish_test