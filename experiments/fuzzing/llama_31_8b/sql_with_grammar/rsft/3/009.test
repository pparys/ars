set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE products(id INTEGER, name TEXT, price REAL);
    INSERT INTO products(id, name, price) VALUES(201, 'Product 1', 10.99);
    INSERT INTO products(id, name, price) VALUES(202, 'Product 2', 5.99);
  }
} {}

do_test select-product-2.1 {
  execsql {
    SELECT name, price FROM products WHERE price > 5.0 ORDER BY name ASC;
  }
} {Product 1 10.99}

finish_test