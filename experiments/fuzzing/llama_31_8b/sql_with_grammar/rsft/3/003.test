set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_id INTEGER, product_name TEXT, price REAL);
    INSERT INTO orders(order_id, customer_id, product_name, price) VALUES(1, 101, 'Product A', 9.99);
    INSERT INTO orders(order_id, customer_id, product_name, price) VALUES(2, 102, 'Product B', 19.99);
  }
} {}

do_test select-product-2.1 {
  execsql {
    SELECT product_name, price FROM orders WHERE customer_id = 101;
  }
} {Product A 9.99}

finish_test