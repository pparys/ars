set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.1 {
  execsql {
    CREATE TABLE books(title TEXT, author TEXT, price REAL);
    INSERT INTO books(title, author, price) VALUES('Book1', 'Author1', 10.99);
    INSERT INTO books(title, author, price) VALUES('Book2', 'Author2', 9.99);
  }
} {}

do_test select-books-3.2 {
  execsql {
    SELECT title, price FROM books WHERE price < 10.0;
  }
} {Book2 9.99}

finish_test