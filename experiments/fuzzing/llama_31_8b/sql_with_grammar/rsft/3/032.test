set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test insert-data-1.0 {
  execsql {
    CREATE TABLE books(id INTEGER, title TEXT, price REAL);
    INSERT INTO books(id, title, price) VALUES(12345, 'SQL Server Tutorial', 19.99);
    INSERT INTO books(id, title, price) VALUES(23456, 'Python Programming', 24.99);
  }
} {}

do_test select-cheapest-book-1.1 {
  execsql {
    SELECT title FROM books ORDER BY price ASC LIMIT 1;
  }
} {SQL Server Tutorial}

finish_test