set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-1.0 {
  execsql {
    CREATE TABLE books(id INTEGER, title TEXT, author TEXT);
    INSERT INTO books VALUES(1, 'Book 1', 'Author 1');
    INSERT INTO books VALUES(2, 'Book 2', 'Author 2');
  }
} {}

do_test select-books-1.1 {
  execsql {
    SELECT title, author FROM books WHERE id = 1;
  }
} {Book 1 Author 1}

finish_test