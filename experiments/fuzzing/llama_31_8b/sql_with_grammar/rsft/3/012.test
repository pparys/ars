do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_name TEXT, order_date DATE);
    INSERT INTO orders(order_id, customer_name, order_date) VALUES(1, 'John Doe', '2022-01-01');
