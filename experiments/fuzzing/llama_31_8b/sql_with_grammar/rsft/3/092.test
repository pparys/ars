set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_name TEXT, order_date DATE);
    INSERT INTO orders(order_id, customer_name, order_date) VALUES(1, 'Customer A', '2022-01-01');
    INSERT INTO orders(order_id, customer_name, order_date) VALUES(2, 'Customer B', '2022-01-15');
  }
} {}

do_test select-latest-order-3.1 {
  execsql {
    SELECT customer_name, order_date 
    FROM orders 
    ORDER BY order_date DESC 
    LIMIT 1;
  }
} {Customer B 2022-01-15}

finish_test