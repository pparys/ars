set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE books(id INTEGER PRIMARY KEY, title TEXT NOT NULL, author TEXT);
    INSERT INTO books(id, title, author) VALUES(1, 'To Kill a Mockingbird', 'Harper Lee');
    INSERT INTO books(title, author) VALUES('1984', 'George Orwell');
    INSERT INTO books(title, author) VALUES('Pride and Prejudice', 'Jane Austen');
  }
} {}

do_test select-books-2.1 {
  execsql {
    SELECT title, author FROM books WHERE id = 3;
  }
} {Pride and Prejudice Jane Austen}

finish_test