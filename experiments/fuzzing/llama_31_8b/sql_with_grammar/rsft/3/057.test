set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-index-2.0 {
  execsql {
    CREATE TABLE scores(username TEXT PRIMARY KEY, score INTEGER);
    INSERT INTO scores VALUES('Alice', 90);
    INSERT INTO scores VALUES('Bob', 95);
  }
} {}

do_test query-index-2.1 {
  execsql {
    SELECT username FROM scores WHERE score = 95 ORDER BY username ASC;
  }
} {Bob}

finish_test