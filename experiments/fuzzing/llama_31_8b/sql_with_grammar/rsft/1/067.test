set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_name TEXT, order_date TEXT);
    INSERT INTO orders(order_id, customer_name, order_date) VALUES(101, 'John Doe', '2022-01-01');
    INSERT INTO orders(order_id, customer_name, order_date) VALUES(102, 'Jane Doe', '2022-01-15');
  }
} {}

do_test select-orders-3.1 {
  execsql {
    SELECT customer_name, order_date FROM orders WHERE order_id = 102;
  }
} {Jane Doe 2022-01-15}

finish_test