set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(
      id INTEGER,
      customer_name TEXT,
      order_date DATE,
      total REAL
    );
    INSERT INTO orders VALUES(1, 'John Doe', '2022-01-01', 100.0);
    INSERT INTO orders VALUES(2, 'Jane Doe', '2022-01-15', 200.0);
  }
} {}

do_test select-orders-2.1 {
  execsql {
    SELECT customer_name, order_date FROM orders
    WHERE total > 150 AND order_date > '2022-01-10';
  }
} {Jane Doe 2022-01-15}

finish_test