set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-autoincrement-2.0 {
  execsql {
    CREATE TABLE contacts(id INTEGER PRIMARY KEY AUTOINCREMENT, email TEXT UNIQUE);
    INSERT INTO contacts(email) VALUES('alice@example.com');
    INSERT INTO contacts(email) VALUES('bob@example.com');
  }
} {}

