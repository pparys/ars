set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE products(category TEXT, name TEXT, price REAL);
    INSERT INTO products(category, name, price) VALUES('Electronics', 'Samsung TV', 1200.99);
    INSERT INTO products(category, name, price) VALUES('Electronics', 'Sony TV', 999.99);
    INSERT INTO products(category, name, price) VALUES('Fashion', 'Shoes', 99.99);
  }
} {}

do_test select-expensive-2.1 {
  execsql {
    SELECT name, category FROM products WHERE price > 1000;
  }
} {Samsung TV Electronics Sony TV Electronics}

finish_test