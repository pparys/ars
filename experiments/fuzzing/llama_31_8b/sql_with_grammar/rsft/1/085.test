    INSERT INTO scores(team, jogador, pontuacao) VALUES('Banana Bunch', 'Jaca Jenkins', 10);
    INSERT INTO scores(team, jogador, pontuacao) VALUES('Banana Bunch', 'Guava Gary', 20);
