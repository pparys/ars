set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE books(id INTEGER, title TEXT, author TEXT);
    INSERT INTO books VALUES(1, 'Book1', 'Author1');
    INSERT INTO books VALUES(2, 'Book2', 'Author2');
  }
} {}

do_test select-books-3.1 {
  execsql {
    SELECT title, author FROM books ORDER BY id;
  }
} {Book1 Author1 Book2 Author2}

finish_test