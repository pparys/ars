set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE customers(id INTEGER, name TEXT, age INTEGER);
    INSERT INTO customers(id, name, age) VALUES(301, 'John', 25);
    INSERT INTO customers(id, name, age) VALUES(302, 'Jane', 30);
  }
} {}

do_test select-name-age-3.1 {
  execsql {
    SELECT name, age FROM customers WHERE id = 301 ORDER BY age DESC LIMIT 1;
  }
} {John 25}

finish_test