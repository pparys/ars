set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_id INTEGER, product_name TEXT);
    INSERT INTO orders(order_id, customer_id, product_name)
      VALUES(300, 100, 'Product X');
    INSERT INTO orders(order_id, customer_id, product_name)
      VALUES(301, 100, 'Product Y');
  }
} {}

do_test select-order-3.1 {
  execsql {
    SELECT product_name FROM orders WHERE customer_id = 100 ORDER BY product_name LIMIT 1;
  }
} {Product X}

finish_test