set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER PRIMARY KEY, customer_name TEXT, order_date DATE);
    INSERT INTO orders(order_id, customer_name, order_date) VALUES(201, 'John Doe', '2022-01-01');
    INSERT INTO orders(order_id, customer_name, order_date) VALUES(202, 'Jane Doe', '2022-01-15');
