set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE products(id INTEGER, category TEXT, price REAL);
    INSERT INTO products(id, category, price) VALUES(1, 'Electronics', 199.99);
    INSERT INTO products(id, category, price) VALUES(2, 'Fashion', 29.99);
    INSERT INTO products(id, category, price) VALUES(3, 'Home', 99.99);
  }
} {}

do_test select-expensive-product-3.1 {
  execsql {
    SELECT category, price FROM products WHERE price > 100 ORDER BY price DESC;
  }
} {Electronics 199.99}

finish_test