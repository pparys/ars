set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test create-immutable-table-1.0 {
  execsql {
    CREATE TABLE IF NOT EXISTS public_data(id INTEGER PRIMARY KEY, name TEXT NOT NULL);
    INSERT INTO public_data(id, name) VALUES(3, 'Charlie');
    INSERT INTO public_data(id, name) VALUES(4, 'David');
  }
} {}

do_test query-immutable-table-1.1 {
  execsql {
    SELECT name FROM public_data WHERE id = 3;
  }
} {Charlie}

finish_test