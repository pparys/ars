set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_id INTEGER, order_date DATE);
    INSERT INTO orders(order_id, customer_id, order_date) 
      VALUES(101, 1, '2022-01-01');
    INSERT INTO orders(order_id, customer_id, order_date) 
      VALUES(102, 2, '2022-01-02');
  }
} {}

do_test sort-order-3.1 {
  execsql {
    SELECT customer_id, order_date 
    FROM orders 
    ORDER BY order_date ASC, customer_id DESC;
  }
} {2 2022-01-02 1 2022-01-01}
finish_test