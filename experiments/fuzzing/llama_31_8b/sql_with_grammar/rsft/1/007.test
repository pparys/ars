set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.1 {
  execsql {
    CREATE TABLE orders(order_id INTEGER PRIMARY KEY, product TEXT, quantity INTEGER);
    INSERT INTO orders(order_id, product, quantity) VALUES(1, 'Product A', 10);
    INSERT INTO orders(order_id, product, quantity) VALUES(2, 'Product B', 20);
  }
} {}

do_test delete-order-2.2 {
  execsql {
    DELETE FROM orders WHERE order_id = 1;
    SELECT order_id FROM orders;
  }
} {2}

finish_test