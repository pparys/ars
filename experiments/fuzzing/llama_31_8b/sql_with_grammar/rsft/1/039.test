set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, customer_id INTEGER, order_date DATE, amount REAL);
    INSERT INTO orders(id, customer_id, order_date, amount) 
      VALUES(1, 101, '2022-01-01', 100.00);
    INSERT INTO orders(id, customer_id, order_date, amount) 
      VALUES(2, 101, '2022-01-15', 200.00);
    INSERT INTO orders(id, customer_id, order_date, amount) 
      VALUES(3, 102, '2022-02-01', 50.00);
  }
} {}

do_test order-by-date-3.1 {
  execsql {
    SELECT customer_id, order_date 
    FROM orders 
    ORDER BY order_date DESC LIMIT 2;
  }
} {101 2022-01-15 101 2022-01-01}

finish_test