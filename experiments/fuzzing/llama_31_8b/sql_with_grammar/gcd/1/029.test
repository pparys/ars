set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE books(id INTEGER, title TEXT, author TEXT, edition INTEGER);
    INSERT INTO books VALUES(1, 'Book1', 'Author1', 1);
    INSERT INTO books VALUES(2, 'Book2', 'Author2', 2);
    INSERT INTO books VALUES(3, 'Book3', 'Author3', 3);
  }
} {}

do_test select-books-ascending-3.1 {
  execsql {
    SELECT title FROM books ORDER BY edition ASC;
  }
} {Book1 Book2 Book3}

finish_test