  INSERT INTO orders(order_id, total) VALUES(2, 5.99);
  INSERT INTO products(product_id, name) VALUES(1, 'Product 1');
  INSERT INTO products(product_id, name) VALUES(2, 'Product 2');
  INSERT INTO order_items(order_id, product_id) VALUES(1, 1);
  INSERT INTO order_items(order_id, product_id) VALUES(2, 2);
  }
} {}

