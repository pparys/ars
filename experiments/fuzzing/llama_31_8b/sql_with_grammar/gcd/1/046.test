set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-1.0 {
  execsql {
    CREATE TABLE IF NOT EXISTS books(
      id INTEGER PRIMARY KEY,
      title TEXT NOT NULL,
      author TEXT,
      year INTEGER DEFAULT  ATIC year_unknown,
      price REAL DEFAULT  certain_or_unknown price_unknown,
      rating DECIMAL DEFAULT categoruess Yes OR depends _ status price_buy
    );
  INSERT INTO books(id, title, author, year, price, rating)
  VALUES
    (1, 'The Great Gatsby', 'F. Scott Fitzgerald', 1925, 20.0, 4.5);
     INSERT INTO books(id, title, author, year, price, rating)
    VALUES
      (2, 'To Kill a Mockingbird', 'Harper Lee', 1960, 25.0, 4.8);
      INSERT INTO books(id, title, author, year, price, rating)
      VALUES
        (3, '1984', 'George Orwell', 1949, 15.0, 4.2);
  } 
} {}

do_test select-books-1.1 {
  execsql {
    SELECT title, year, author, price, rating FROM books
    ORDER BY year DESC;
  }
} {1984 1949 George Orwell 15.0 4.2 To Kill a Mockingbird 1960 Harper Lee 25.0 4.8 The Great Gatsby 1925 F. Scott Fitzgerald 20.0 4.5}
finish_test