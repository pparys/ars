set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-3.0 {
  execsql {
    CREATE TABLE books(id INTEGER PRIMARY KEY, title TEXT, published INTEGER);
    INSERT INTO books VALUES(1, 'Book1', 2010);
    INSERT INTO books VALUES(2, 'Book2', 2011);
    INSERT INTO books VALUES(3, 'Book3', 2012);
  }
} {}
 
do_test query-3.1 {
  execsql {
    SELECT title, published FROM books ORDER BY published LIMIT 1;
  }
} {Book1 2010}

finish_test