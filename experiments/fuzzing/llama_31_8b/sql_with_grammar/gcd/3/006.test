set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, customer TEXT, total REAL);
    INSERT INTO orders(id, customer, total) VALUES(201, 'John Doe', 100.99);
    INSERT INTO orders(id, customer, total) VALUES(202, 'Jane Doe', 50.00);
  }
} {}

do_test delete-order-2.1 {
  execsql {
    DELETE FROM orders WHERE customer = 'Jane Doe';
    SELECT * FROM orders;
  }
} {201 John Doe 100.99 202 Jane Doe 50}

finish_test