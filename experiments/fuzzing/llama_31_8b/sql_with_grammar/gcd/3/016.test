set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(id INTEGER PRIMARY KEY, customer TEXT, total REAL);
    INSERT INTO orders(id, customer, total) VALUES(101, 'Alice', 100.0);
    INSERT INTO orders(id, customer, total) VALUES(102, 'Bob', 200.0);
  }
} {}

do_test select-order-2.1 {
  execsql {
    SELECT customer, total FROM orders WHERE total > 150;
  }
} {Bob 200.0}

finish_test