do_test setup-group-2.0 {
  execsql {
    CREATE TABLE numbers(
      id INTEGER PRIMARY KEY,
      value INTEGER
    );

    INSERT INTO numbers(id, value) VALUES(1, 1);
    INSERT INTO numbers(id, value) VALUES(2, 2);
    INSERT INTO numbers(id, value) VALUES(3, 3);
  }
} {}

