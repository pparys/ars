set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-1.0 {
  execsql {
    CREATE TABLE employees(id INTEGER, name TEXT, age INTEGER, job_name TEXT);
    INSERT INTO employees VALUES(1, 'John', 25, 'Engineer');
    INSERT INTO employees VALUES(2, 'Alice', 30, 'Manager');
    INSERT INTO employees VALUES(3, 'Bob', 35, 'Developer');
  }
} {}

do_test select-max-age-1.1 {
  execsql {
    SELECT MAXAGE AS MaxAge FROM employees
    ORDER BY Age
    LIMIT 1;
  }
} {35}

finish_test