set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE books(id INTEGER, title TEXT, author TEXT);
    INSERT INTO books(id, title, author) VALUES(201, 'Book1', 'AuthorA');
    INSERT INTO books(id, title, author) VALUES(202, 'Book2', 'AuthorB');
  }
} {}

do_test group-by-author-2.1 {
  execsql {
    SELECT author FROM books GROUP BY author;
  }
} {AuthorA AuthorB}

finish_test