set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setupiei-1.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, order_date DATE);
    CREATE TABLE customers(id INTEGER, name TEXT);
    INSERT INTO orders VALUES(1, '2022-01-01');
    INSERT INTO orders VALUES(2, '2022-02-02');
    INSERT INTO customers VALUES(101, 'John');
    INSERT INTO customers VALUES(102, 'Jane');
  }
} {}

do_test select-orders-1.1 {
  execsql {
    SELECT o.* FROM orders AS o
    WHERE oorder_date = '2022-01-01'
    GROUP BY oorder_date
    LIMIT 1;
  }
} {1 2022-01-01}

finish_test