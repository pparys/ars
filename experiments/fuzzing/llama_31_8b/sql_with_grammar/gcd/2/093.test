set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer TEXT, date DATE);
    INSERT INTO orders(order_id, customer, date) VALUES(101, 'Alice', '2022-01-01');
    INSERT INTO orders(order_id, customer, date) VALUES(102, 'Bob', '2022-01-02');
  }
} {}

