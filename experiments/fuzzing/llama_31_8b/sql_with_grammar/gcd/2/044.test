set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE messages(id INTEGER, content TEXT, created_at DATETIME);
    INSERT INTO messages(id, content, created_at) VALUES(1, 'Hello', '2022-01-01 12:00:00');
    INSERT INTO messages(id, content, created_at) VALUES(2, 'World', '2022-01-02 13:00:00');
  }
} {}

do_test query-sort-2.1 {
  execsql {
    SELECT content, created_at
    FROM messages
    ORDER BY created_at DESC
  ;}
} {World 2022-01-02 13:00:00 Hello 2022-01-01 12:00:00}

finish_test