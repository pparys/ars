set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-constraints-3.0 {
  execsql {
    CREATE TABLE addresses(id INTEGER, street TEXT, city TEXT, state TEXT, zip TEXT);
    INSERT INTO addresses VALUES(1, '123 Main St', 'Anytown', 'CA', '12345');
    INSERT INTO addresses VALUES(2, '456 Elm St', 'Othertown', 'NY', '67890');
  }
} {}

do_test update-addresses-3.1 {
  execsql {
    UPDATE addresses SET 
      street = '789 Oak St', 
      city = 'Othertown', 
      state = 'CA', 
      zip = '12345' 
    WHERE id = 2;
  }
} {2}

finish_test