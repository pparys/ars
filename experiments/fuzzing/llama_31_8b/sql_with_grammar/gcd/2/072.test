set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-1.0 {
  execsql {
    CREATE TABLE books (
      id INTEGER PRIMARY KEY AUTOINCREMENT, 
      title TEXT NOT NULL,
      author TEXT NOT NULL,
      year INTEGER NOT NULL
    );
  INSERT INTO books(title, author, year) VALUES('Book A', 'Author A', 2000);
  INSERT INTO books(title, author, year) VALUES('Book B', 'Author B', 2001);
  }
} {}

do_test insert-book-1.1 {
  execsql {
    SELECT id, title, author, year FROM books ORDER BY id;
  }
} {1 Book A Author A 2000 2 Book B Author B 2001}

finish_test