<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE customers [
  <!ELEMENT customers (customer*)>
  <!ELEMENT customer (name,age,order*)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT age (#PCDATA)>
  <!ELEMENT order EMPTY>
  <!ATTLIST order
    id CDATA #REQUIRED
    total CDATA #IMPLIED>
]>
<customers>
  <customer>
    <name>Jane Smith</name>
    <age>30</age>
    <order id="123" total="1000"/>
    <order id="456" total="500"/>
  </customer>
  <customer>
    <name>John Doe</name>
    <age>35</age>
    <order id="789" total="1200"/>
  </customer>
</customers>