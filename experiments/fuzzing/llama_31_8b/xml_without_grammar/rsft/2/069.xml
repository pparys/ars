<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE catalog [
  <!ELEMENT catalog (product*)>
  <!ELEMENT product (description+, price)>
  <!ELEMENT description (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ATTLIST product
    id CDATA #REQUIRED
    brand CDATA #IMPLIED>
]>
<catalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="catalog.xsd">
  <product id="P001" brand="Sony">
    <description>10.1" LCD TV</description>
    <price>$1,500.00</price>
  </product>
  <product id="P002" brand="Samsung">
    <description>8.9" Tablet</description>
    <price>$799.99</price>
  </product>
</catalog>