<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE library [
  <!ELEMENT library (genre+)>
  <!ELEMENT genre (#PCDATA)>
  <!ELEMENT book (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED
    author CDATA #IMPLIED>
]>
<library>
  <genre>Fiction</genre>
  <genre>Non-Fiction</genre>
  <book id="BK1" author="John Doe">Introduction to XML</book>
  <book id="BK2">XML Generation</book>
</library>