<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE customer [
  <!ELEMENT customer (name, address, phones+)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT phones (#PCDATA)|
     (phone+)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST customer
    id CDATA #REQUIRED
    email CDATA #IMPLIED>
]>
<customer id="CUST001" email="john.doe@example.com">
  <name>John Doe</name>
  <address>
    <street>123 Main St</street>
    <city>Anytown</city>
    <state>CA</state>
    <zip>12345</zip>
  </address>
  <phones>(415) 123-4567 (415) 901-2345</phones>
  <phone>(403) 789-0123</phone>
  <phone>(510) 111-2222</phone>
</customer>