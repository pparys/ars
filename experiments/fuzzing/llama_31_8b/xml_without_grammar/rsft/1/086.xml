<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE bookstore [
  <!ELEMENT bookstore (book | cd | author | genre)>
  <!ELEMENT book (title, price, year, author)>
  <!ELEMENT cd (title, artist, price)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT genre (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED
    isbn CDATA #IMPLIED>
  <!ATTLIST cd
    id CDATA #REQUIRED
    label CDATA #IMPLIED>
]>
<bookstore>
  <author>John Grisham</author>
  <genre>Thriller</genre>
  <cd id="BOOK001" label="Sony">
    <title>Dune</title>
    <artist>Frank Herbert</artist>
    <price>14.99</price>
  </cd>
  <book id="BOOK002" isbn="1234567890">
    <title>The Shining</title>
    <price>12.99</price>
    <year>1980</year>
    <author>Stephen King</author>
  </book>
</bookstore>