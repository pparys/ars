<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE library [
  <!ELEMENT library (book*)>
  <!ELEMENT book (title, author, publicationDate)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT publicationDate (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED
    genre CDATA #IMPLIED
    >
]>
<library>
  <book id="bk101" genre="mystery">
    <title>The Mysterious Affair at Styles</title>
    <author>Agatha Christie</author>
    <publicationDate>1920</publicationDate>
  </book>
  <book id="bk102" genre="romance">
    <title>Midnight</title>
    <author>Isaac Waldfang</author>
    <publicationDate>1987</publicationDate>
  </book>
</library>