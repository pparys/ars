<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE catalog [
  <!ELEMENT catalog (author+, item*)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT item (#PCDATA)>
  <!ATTLIST item
    id (A|B|C) #REQUIRED
    price CDATA #REQUIRED>
]>
<catalog>
  <author>John</author>
  <author>Jane</author>
  <item id='A' price='10.99'>Book A</item>
  <item id='B' price='5.99'>Book B</item>
  <item id='C' price='7.99'>Book C</item>
</catalog>