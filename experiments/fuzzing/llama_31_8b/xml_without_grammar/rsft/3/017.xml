<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE books [
  <!ELEMENT books (book+)>
  <!ELEMENT book (author, title, year, publisher)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT year (#PCDATA)>
  <!ELEMENT publisher (#PCDATA)>
  <!ATTLIST book
    id ID #REQUIRED
    title CDATA #IMPLIED>
]>
<books>
  <book id="bk101" title="title101" year="2015" publisher="publisher101">
    <author>Aaron</author>
  </book>
  <book id="bk102" title="title102" year="2016" publisher="publisher102">
    <author>Brian</author>
  </book>
</books>