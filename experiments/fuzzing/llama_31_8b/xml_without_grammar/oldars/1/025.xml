<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE customers [
  <!ELEMENT customers (customer*)>
  <!ELEMENT customer (name, address, phone*)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST customer
    idCDATA #REQUIRED>
]>
<customers>
  <customer id="123">
    <name>John Doe</name>
    <address>
      <street>123 Main St</street>
      <city>Anytown</city>
      <state>CA</state>
      <zip>12345</zip>
    </address>
    <phone>123-456-7890</phone>
    <phone>987-654-3210</phone>
  </customer>
  <customer id="456">
    <name>Jane Smith</name>
    <address>
      <street>456 Elm St</street>
      <city>Othertown</city>
      <state>NY</state>
      <zip>67890</zip>
    </address>
    <phone>555-123-4567</phone>
  </customer>
</customers>