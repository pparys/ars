  </book>
  <book id="bk102" title="Another Book Title">
    <author>Jane Doe</author>
    <price>5.95</price>
    <price>7.99</price>
  </book>
  <book id="bk103" title="Third Book Title">
    <author>Bob Brown</author>
  </book>
</catalog>