<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE catalog [
  <!ELEMENT catalog (book*)>
  <!ELEMENT book (author,price*,description?)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT price (#CDATA)>
  <!ELEMENT description (#PCDATA)>
  <!ATTLIST book
    id ID #REQUIRED
    title CDATA #REQUIRED>
]>
