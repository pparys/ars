<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE cart [
  <!ELEMENT cart (product+, customer)>
  <!ELEMENT product (description, price)>
  <!ELEMENT customer (name, address)>
  <!ELEMENT description (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (#PCDATA)>
  <!ATTLIST product
    id CDATA #REQUIRED>
]>
<cart>
  <product id="prod1">
    <description>Example Product 1</description>
    <price>19.99</price>
  </product>
  <product id="prod2">
    <description>Example Product 2</description>
    <price>9.99</price>
  </product>
  <customer>
    <name>John Doe</name>
    <address>123 Main St.</address>
  </customer>
</cart>