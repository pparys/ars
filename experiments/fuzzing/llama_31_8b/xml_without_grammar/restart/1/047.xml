<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE customers [
  <!ELEMENT customers (customer*)>
  <!ELEMENT customer (name, address, phone)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (\* | (street, city, state, zip))>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST customer
    id CDATA #REQUIRED>
]>
<customers>
  <customer id="cust1">
    <name>Customer1</name>
    <address>
      <street>123 Main St</street>
      <city>New York</city>
      <state>NY</state>
      <zip>10001</zip>
    </address>
    <phone>212-123-4567</phone>
  </customer>
  <customer id="cust2">
    <name>Customer2</name>
    <address>
      <street>456 Broadway</street>
    </address>
    <phone>203-987-6543</phone>
  </customer>
</customers>