<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE books [
  <!ELEMENT books (book*)>
  <!ELEMENT book (title,author)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT author (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED
    genre CDATA #IMPLIED
    publication-year CDATA #IMPLIED>
]>
<books>
  <book id="book1" genre="fiction" publication-year="2020">
    <title>The Great Gatsby</title>
    <author>F. Scott Fitzgerald</author>
  </book>
  <book id="book2">
    <title>To Kill a Mockingbird</title>
    <author>Harper Lee</author>
  </book>
  <book id="book3" genre="non-fiction" publication-year="2015">
    <title>The Immortal Life of Henrietta Lacks</title>
    <author>Renee Wegrzyn</author>
  </book>
</books>