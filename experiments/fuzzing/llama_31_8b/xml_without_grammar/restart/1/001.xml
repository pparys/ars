<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE catalog [
  <!ELEMENT catalog (author, publisher, booklist)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT publisher (#PCDATA)>
  <!ELEMENT booklist (book+)>
  <!ELEMENT book (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED
    title CDATA #REQUIRED
  >
]>
<catalog>
  <author>John Smith</author>
  <publisher>Pearson Education</publisher>
  <booklist>
    <book id="bk001" title="Introduction to XML"/>
    <book id="bk002" title="XML Tutorials"/>
    <book id="bk003" title="XML for Beginners"/>
  </booklist>
</catalog>