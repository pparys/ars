<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE students [
  <!ELEMENT students (student*)>
  <!ELEMENT student (name,age,grades)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT age (#PCDATA)>
  <!ELEMENT grades (grade+)>
  <!ELEMENT grade (#PCDATA)>
  <!ATTLIST student
    id CDATA #REQUIRED>
]>
<students>
  <student id="1">
    <name>Jane Doe</name>
    <age>25</age>
    <grades>
      <grade>90</grade>
      <grade>85</grade>
    </grades>
  </student>
  <student id="2">
    <name>John Smith</name>
    <age>30</age>
    <grades>
      <grade>95</grade>
      <grade>92</grade>
    </grades>
  </student>
</students>