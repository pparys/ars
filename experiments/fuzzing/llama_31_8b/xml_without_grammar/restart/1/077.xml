<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE users [
  <!ELEMENT users (user*)>
  <!ELEMENT user (name, age, address)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT age (#PCDATA)>
  <!ELEMENT address (street, city, state)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ATTLIST user
    id CDATA #IMPLIED
  >
]>
<users>
  <user id="1">
    <name>John Doe</name>
    <age>35</age>
    <address>
      <street>123 Main St</street>
      <city>New York</city>
      <state>NY</state>
    </address>
  </user>
  <user id="2">
    <name>Jane Doe</name>
    <age>30</age>
    <address>
      <street>456 Elm St</street>
      <city>Los Angeles</city>
      <state>CA</state>
    </address>
  </user>
</users>