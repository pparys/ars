<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE employees [
  <!ELEMENT employees (employee*)>
  <!ELEMENT employee (name, address, contact)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (#PCDATA)>
  <!ATTLIST address 
    city CDATA #REQUIRED
    country CDATA #REQUIRED>
  <!ATTLIST contact 
    phone CDATA #IMPLIED
    email CDATA #IMPLIED>
]>
<employees>
  <employee>
    <name>John Doe</name>
    <address street="123 Main St" city="Anytown" country="USA"/>
    <contact phone="123-456-7890" email="johndoe@example.com"/>
  </employee>
  <employee>
    <name>Jane Smith</name>
    <address street="456 Elm St" city="Othertown" country="Canada"/>
    <contact phone="098-765-4321"/>
  </employee>
</employees>