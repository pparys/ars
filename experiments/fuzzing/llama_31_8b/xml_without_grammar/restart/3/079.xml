<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE customers [
  <!ELEMENT customers (customer*)>
  <!ELEMENT customer (name,address)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street,city,state,zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ATTLIST customer
    id CDATA #REQUIRED
  >
]>
<customers>
  <customer id="C001">
    <name>John Doe</name>
    <address>
      <street>123 Main St</street>
      <city>New York</city>
      <state>NY</state>
      <zip>10001</zip>
    </address>
  </customer>
  <customer id="C002">
    <name>Jane Doe</name>
    <address>
      <street>456 Park Ave</street>
      <city>Chicago</city>
      <state>IL</state>
      <zip>60601</zip>
    </address>
  </customer>
</customers>