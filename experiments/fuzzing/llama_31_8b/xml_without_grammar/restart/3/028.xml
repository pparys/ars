<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE addresses [
  <!ELEMENT addresses (address+)>
  <!ELEMENT address (name, street, city, state, zip)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ATTLIST address
    id CDATA #IMPLIED>
]>
<addresses>
  <address>
    <name>John Doe</name>
    <street>123 Main St</street>
    <city>Anytown</city>
    <state>CA</state>
    <zip>12345</zip>
  </address>
  <address id="second-address">
    <name>Jane Doe</name>
    <street>456 Elm St</street>
    <city>Othertown</city>
    <state>NY</state>
    <zip>67890</zip>
  </address>
</addresses>