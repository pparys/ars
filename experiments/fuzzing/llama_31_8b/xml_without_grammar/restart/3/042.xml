<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE person [
  <!ELEMENT person (name, phone*, address)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT phone (type, number)>
  <!ELEMENT type (#PCDATA)>
  <!ELEMENT number (#PCDATA)>
  <!ELEMENT address (#PCDATA)>
  <!ATTLIST person
    id CDATA #IMPLIED
    type (personal|professional) "personal">
]>
<person id="1" type="personal">
  <name>John Doe</name>
  <phone>
    <type>Cell</type>
    <number>123-456-7890</number>
  </phone>
  <phone>
    <type>Home</type>
    <number>987-654-3210</number>
  </phone>
  <address>123 Main St</address>
</person>