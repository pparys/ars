<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE addressBook [
  <!ELEMENT addressBook (address*)>
  <!ELEMENT address (name,phone,street,country)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT country (#PCDATA)>
  <!ATTLIST address
    type CDATA #REQUIRED
    date CDATA #IMPLIED>
]>
<addressBook>
  <address type="home" date="2022-01-01">
    <name>John Doe</name>
    <phone>1234567890</phone>
    <street>Main Street</street>
    <country>USA</country>
  </address>
  <address type="work" date="2022-01-01">
    <name>Jane Doe</name>
    <phone>9876543210</phone>
    <street>Office Street</street>
    <country>USA</country>
  </address>
</addressBook>