<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE music [
  <!ELEMENT music (track+)>
  <!ELEMENT track ((artist, album, year) | (album, year, artist))>
  <!ELEMENT artist (#PCDATA)>
  <!ELEMENT album (#PCDATA)>
  <!ELEMENT year (#PCDATA)>
  <!ATTLIST music
    title CDATA #REQUIRED
    genre CDATA #IMPLIED>
]>
<music title="Rock Anthems" genre="Rock">
  <track>
