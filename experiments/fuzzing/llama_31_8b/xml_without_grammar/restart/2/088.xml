<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE addressBook [
  <!ELEMENT addressBook (address+)>
  <!ELEMENT address (#PCDATA|name|phone|email+)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ELEMENT email (#PCDATA)>
  <!ATTLIST address
    id CDATA #REQUIRED
    type (home|work) #REQUIRED>
]>
<addressBook>
    <address id="1" type="home">
        <name>John Doe</name>
        <phone>123-456-7890</phone>
        <email>johndoe@example.com</email>
    </address>
    <address id="2" type="work">
        <name>Jane Doe</name>
        <phone>987-654-3210</phone>
        <email>janedoe@example.com</email>
    </address>
</addressBook>