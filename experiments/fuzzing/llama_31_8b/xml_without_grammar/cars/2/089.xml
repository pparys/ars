<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE person [
  <!ELEMENT person (name, email*, phone+)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT email (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST person
    id CDATA #REQUIRED>
]>
<person id="123">
  <name>John Doe</name>
  <email>johndoe@example.com</email>
  <phone>123-456-7890</phone>
  <phone>098-765-4321</phone>
</person>