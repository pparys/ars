<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE cart [
  <!ELEMENT cart (product*)>
  <!ELEMENT product (description, price, image)>
  <!ELEMENT description (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ELEMENT image (#PCDATA)>
  <!ATTLIST product
    id CDATA #REQUIRED
    quantity CDATA #REQUIRED
  >  
   <!ATTLIST cart
    customer_id CDATA #REQUIRED
  > 
]>
<cart customer_id="C001">
  <product id="P001" quantity="2">
    <description>Product Description 1</description>
    <price>19.99</price>
    <image>image1.jpg</image>
  </product>
  <product id="P002" quantity="5">
    <description>Product Description 2</description>
    <price>9.99</price>
    <image>image2.jpg</image>
  </product>
  <product id="P003" quantity="3">
    <description>Product Description 3</description>
    <price>29.99</price>
    <image>image3.jpg</image>
  </product>
</cart>