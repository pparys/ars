<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE student [
  <!ELEMENT student (name,age,school,degrees)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT age (#PCDATA)>
  <!ELEMENT school (name,street,city,region,country)>
  <!ELEMENT degrees (degree+)>
  <!ELEMENT degree (#PCDATA)>
  <!ATTLIST student
    id CDATA #REQUIRED
    type (undergraduate|graduate) "undergraduate">
]>
<student id="1234" type="graduate">
  <name>John Doe</name>
  <age>30</age>
  <school>
    <name>University of New York</name>
    <street>Main Street</street>
    <city>New York</city>
    <region>NY</region>
    <country>USA</country>
  </school>
  <degrees>
    <degree>BSc in Computer Science</degree>
    <degree>MSc in Data Science</degree>
  </degrees>
</student>