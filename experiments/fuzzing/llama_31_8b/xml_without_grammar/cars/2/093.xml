<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE library [
  <!ELEMENT library (book+)>
  <!ELEMENT book (author+, publisher+, title+)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT publisher (#PCDATA)>
  <!ELEMENT title (#PCDATA)>
  <!ATTLIST book
    id ID #REQUIRED
    year CDATA #IMPLIED
  >
]>
<library>
  <book id="bk101" year="1990">
    <author>John Smith</author>
    <publisher>O'Reilly</publisher>
    <title>XML in 10 Simple Steps</title>
  </book>
  <book id="bk102" year="2000">
    <author>Jane Smith</author>
    <publisher>NOS-GO</publisher>
    <title>The XML Advantage</title>
  </book>
</library>