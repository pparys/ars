<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE library [
  <!ELEMENT library (book+)>
  <!ELEMENT book (title, author, publicationDate)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT publicationDate (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED
    genre CDATA #IMPLIED>
]>
<library>
  <book id="bk101" genre="fiction">
    <title>The Great Gatsby</title>
    <author>F. Scott Fitzgerald</author>
    <publicationDate>1925</publicationDate>
  </book>
  <book id="bk102" genre="historical">
    <title>The Wars of the Roses</title>
    <author>Roger Gallo</author>
    <publicationDate>1999</publicationDate>
  </book>
</library>