<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE library [
  <!ELEMENT library (book+, author+)>
  <!ELEMENT book (title, author+, price)>
  <!ELEMENT author (name, contact)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT contact (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED>
  <!ATTLIST author
    id CDATA #REQUIRED>
]>
<library>
  <book id="bk101">
    <title>XML for Beginners</title>
    <price>39.95</price>
  </book>
  <book id="bk102">
    <title>XML for Dummies</title>
    <price>59.95</price>
  </book>
  <author id="a1">
    <name>John Doe</name>
    <contact>john.doe@example.com</contact>
  </author>
  <author id="a2">
    <name>Jane Doe</name>
    <contact>jane.doe@example.com</contact>
  </author>
</library>