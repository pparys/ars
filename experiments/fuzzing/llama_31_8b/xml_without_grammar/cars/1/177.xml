<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE customers [
  <!ELEMENT customers (customer*)>
  <!ELEMENT customer (name, address, contact*)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT contact (#PCDATA)>
  <!ATTLIST contact
    type (phone|email) #REQUIRED
    value CDATA #REQUIRED>
]>
<customers>
  <customer>
    <name>John Doe</name>
    <address>
      <street>123 Main St</street>
      <city>New York</city>
      <state>NY</state>
      <zip>10001</zip>
    </address>
    <contact type="phone" value="123-456-7890"/>
    <contact type="email" value="john.doe@example.com"/>
  </customer>
  <customer>
    <name>Jane Doe</name>
    <address>
      <street>456 Park Ave</street>
      <city>New York</city>
      <state>NY</state>
      <zip>10001</zip>
    </address>
    <contact type="phone" value="987-654-3210"/>
    <contact type="email" value="jane.doe@example.com"/>
  </customer>
</customers>