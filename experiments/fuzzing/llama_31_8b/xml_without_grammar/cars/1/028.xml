<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE students [
  <!ELEMENT students (student*)>
  <!ELEMENT student (name, course, grade, address)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT course (#PCDATA)>
  <!ELEMENT grade (#PCDATA)>
  <!ELEMENT address (street, city, state)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ATTLIST student
    id CDATA #REQUIRED
  >
]>
<students>
  <student id="st1">
    <name>John Doe</name>
    <course>Math</course>
    <grade>A</grade>
    <address>
      <street>123 Main St</street>
      <city>New York</city>
      <state>NY</state>
    </address>
  </student>
  <student id="st2">
    <name>Jane Doe</name>
    <course>English</course>
    <grade>B</grade>
    <address>
      <street>456 Elm St</street>
      <city>Los Angeles</city>
      <state>CA</state>
    </address>
  </student>
</students>