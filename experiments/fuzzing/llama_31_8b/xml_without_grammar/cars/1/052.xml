<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE catalog [
  <!ELEMENT catalog (product*)>
  <!ELEMENT product (description,pricehumidity*)>
  <!ELEMENT description (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ELEMENT humidity (#PCDATA)>
  <!ATTLIST product
    id ID #REQUIRED
    type (user|jabber|xml) "jabber"
  >
]>
<catalog>
  <product id="1" type="jabber">
    <description>This is a product description.</description>
    <price>100.00</price>
    <humidity>60%</humidity>
  </product>
  <product id="2" type="jabber">
    <description>This is another product description.</description>
    <price>200.00</price>
    <humidity>70%</humidity>
  </product>
</catalog>