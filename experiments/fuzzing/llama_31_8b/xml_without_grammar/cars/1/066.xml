<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE students [
  <!ELEMENT students (student*)>
  <!ELEMENT student (name, age, contact)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT age (#PCDATA)>
  <!ELEMENT contact (#PCDATA)>
  <!ATTLIST student
    id CDATA #REQUIRED
  >
]>
<students>
  <student id="S1">
    <name>John Doe</name>
    <age>25</age>
    <contact>1234567890</contact>
  </student>
  <student id="S2">
    <name>Jane Doe</name>
    <age>30</age>
    <contact>0987654321</contact>
  </student>
</students>