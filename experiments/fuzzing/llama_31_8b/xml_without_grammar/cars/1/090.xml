<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE bookstore [
  <!ELEMENT bookstore (book)>
  <!ELEMENT book (title,price,author)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ELEMENT author (#PCDATA)>
  <!ATTLIST book
    id ID #REQUIRED
    publication-date CDATA #REQUIRED>
  <!ATTLIST bookstore
    name CDATA #IMPLIED
    location CDATA #IMPLIED>
]>
<bookstore name="ABC Bookstore" location="New York">
  <book id="BK101" publication-date="2010-01-01">
    <title>The Great Gatsby</title>
    <price>19.95</price>
    <author>F. Scott Fitzgerald</author>
  </book>
  <book id="BK102" publication-date="1920-01-01">
    <title>The Catcher in the Rye</title>
    <price>14.95</price>
    <author>J.D. Salinger</author>
  </book>
</bookstore>