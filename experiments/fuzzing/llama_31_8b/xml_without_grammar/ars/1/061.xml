<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE users [
  <!ELEMENT users (user+)>
  <!ELEMENT user (name, address)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (#PCDATA)>
  <!ATTLIST user
    id CDATA #IMPLIED>
]>
<users>
  <user id="1">
    <name>Alice</name>
    <address>123 Main St, Anytown, USA</address>
  </user>
  <user id="2">
    <name>Bob</name>
    <address>456 Elm St, Othertown, USA</address>
  </user>
</users>