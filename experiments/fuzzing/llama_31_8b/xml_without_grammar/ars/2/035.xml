<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE people [
  <!ELEMENT people (person*)>
  <!ELEMENT person (name, address, contact*)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (#PCDATA)>
  <!ELEMENT contact EMPTY>
  <!ATTLIST contact
    type (phone|email) #REQUIRED
  >
]>
<people>
  <person>
    <name>John Doe</name>
    <address>123 Main St</address>
    <contact type="phone">123-456-7890</contact>
    <contact type="email">john@example.com</contact>
  </person>
  <person>
    <name>Jane Smith</name>
    <address>456 Elm St</address>
    <contact type="phone">098-765-4321</contact>
  </person>
</people>