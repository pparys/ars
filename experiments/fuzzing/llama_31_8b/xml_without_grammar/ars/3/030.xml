<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE catalog [
  <!ELEMENT catalog (book+, category*)>
  <!ELEMENT book (#PCDATA)>
  <!ELEMENT category (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED
    price CDATA #IMPLIED>
  <!ATTLIST category
    id CDATA #REQUIRED
  >
]>
<catalog>
  <book id="bk101" price="29.99">Book#1</book>
  <book id="bk102">Book#2</book>
  <category id="cat1">Fiction</category>
</catalog>