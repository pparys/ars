<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE employees [
  <!ELEMENT employees (employee+)>
  <!ELEMENT employee (name, address)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (#PCDATA)>
  <!ATTLIST employee
    id ID #REQUIRED
  >
]>
<employees>
  <employee id="E001">
    <name>John Doe</name>
    <address>123 Main St, Anytown USA</address>
  </employee>
  <employee id="E002">
    <name>Jane Doe</name>
    <address>456 Park Ave, Anytown USA</address>
  </employee>
</employees>