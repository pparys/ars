<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE employees [
  <!ELEMENT employees (employee*)>
  <!ELEMENT employee (address|phone|email)*>
  <!ELEMENT address (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ELEMENT email (#PCDATA)>
  <!ATTLIST employee
    id CDATA #REQUIRED
    name CDATA #REQUIRED>
  <!ATTLIST address
    street CDATA #REQUIRED
    city CDATA #REQUIRED
    state CDATA #REQUIRED
    zip CDATA #REQUIRED>
]>
<employees>
  <employee id="emp-101" name="John Doe">
    <phone>123-456-7890</phone>
    <address street="123 Main St" city="New York" state="NY" zip="10001"/>
  </employee>
  <employee id="emp-102" name="Jane Doe">
    <email>jane@example.com</email>
    <address street="456 Broadway" city="Los Angeles" state="CA" zip="90013"/>
  </employee>
</employees>