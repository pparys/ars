<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE employee [

  <!ELEMENT employee (name, address, contact)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT contact (phone, email)>
  <!ELEMENT phone (#PCDATA)>
  <!ELEMENT email (#PCDATA)>

  <!ATTLIST employee
    id (emp-10|emp-20) #REQUIRED
  >
]>
<employee id="emp-10">
  <name>John Doe</name>
  <address>
    <street>123 Main St</street>
    <city>Anytown</city>
    <state>CA</state>
    <zip>12345</zip>
  </address>
  <contact>
    <phone>555-1234</phone>
    <email>johndoe@example.com</email>
  </contact>
</employee>