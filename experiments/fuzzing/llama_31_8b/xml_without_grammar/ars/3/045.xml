<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config [
  <!ELEMENT user (name, age, address)>
  <!ELEMENT address (street, city)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT age (#PCDATA)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ATTLIST user id CDATA #REQUIRED>
]>
<config>
  <user id="12345">
    <name>John Doe</name>
    <age>30</age>
    <address>
      <street>123 Main St</street>
      <city>New York</city>
    </address>
  </user>
</config>