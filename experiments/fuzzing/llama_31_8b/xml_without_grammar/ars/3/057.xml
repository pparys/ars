<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE list [
  <!ELEMENT list (person+)>
  <!ELEMENT person (name, age)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT age (#PCDATA)>
  <!ATTLIST person
    id CDATA #REQUIRED
  >
]>
<list>
  <person id="1">
    <name>John Doe</name>
    <age>30</age>
  </person>
  <person id="2">
    <name>Jane Doe</name>
    <age>25</age>
  </person>
</list>