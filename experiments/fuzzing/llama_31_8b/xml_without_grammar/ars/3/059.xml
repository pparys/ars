<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE customer [
  <!ELEMENT customer (address,contact*,order+)>
  <!ELEMENT address (street,city,state,country)>
  <!ELEMENT contact (name,phone,email)>
  <!ELEMENT order (product#,quantity)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT country (#PCDATA)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ELEMENT email (#PCDATA)>
  <!ELEMENT product (#PCDATA)>
  <!ATTLIST order
    id CDATA #REQUIRED
    date CDATA #IMPLIED>
]>
<customer>
  <address>
    <street>Main Street</street>
    <city>New York</city>
    <state>NY</state>
    <country>USA</country>
  </address>
  <contact>
    <name>John Doe</name>
    <phone>123-456-7890</phone>
    <email>johndoe@email.com</email>
  </contact>
  <contact>
    <name>Jane Doe</name>
    <phone>987-654-3210</phone>
    <email>janedoe@email.com</email>
  </contact>
  <order id="ORD-1" date="2024-07-16">
    <product>Guitar</product>
    <quantity>2</quantity>
  </order>
  <order id="ORD-2" date="2024-07-17">
    <product>Drum</product>
    <quantity>1</quantity>
  </order>
</customer>