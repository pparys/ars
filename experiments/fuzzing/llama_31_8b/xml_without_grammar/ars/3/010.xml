<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE library [
  <!ELEMENT library (book+)>
  <!ELEMENT book (author+, price?, publicationDate?)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ELEMENT publicationDate (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED>
]>
<library>
    <book id="1">
        <author>John Smith</author>
        <price>24.99</price>
        <publicationDate>2020-01-01</publicationDate>
    </book>
    <book id="2">
        <author>Jane Doe</author>
        <price>19.99</price>
        <publicationDate>2021-01-01</publicationDate>
    </book>
</library>