<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE users [
  <!ELEMENT users (user+)>
  <!ELEMENT user (name, address)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ATTLIST user
    id CDATA #REQUIRED>
]>
<users>
  <user id="1">
    <name>John Smith</name>
    <address>
      <street>123 Main St</street>
      <city>New York</city>
      <state>NY</state>
      <zip>10001</zip>
    </address>
  </user>
  <user id="2">
    <name>Jane Doe</name>
    <address>
      <street>456 Elm St</street>
      <city>Chicago</city>
      <state>IL</state>
      <zip>60601</zip>
    </address>
  </user>
</users>