<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE books [
  <!ELEMENT books (author,book*)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT book (title,price,author)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ATTLIST book
    genre (fiction|non-fiction) #REQUIRED
    publication-year CDATA #IMPLIED
  >
]>
<books>
  <author>John Smith</author>
  <book genre="fiction" publication-year="2010">
    <title>The Great Gatsby</title>
    <price>$25.99</price>
    <author>John Smith</author>
  </book>
  <book genre="non-fiction" publication-year="2020">
    <title>The Science of Happiness</title>
    <price>$19.99</price>
    <author>Jane Doe</author>
  </book>
</books>