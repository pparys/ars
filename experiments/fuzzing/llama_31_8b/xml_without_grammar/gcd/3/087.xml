<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE users [
  <!ELEMENT users (user*)>
  <!ELEMENT user (name, address, phone)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST user
    id CDATA #IMPLIED>
  <!ATTLIST users
    version CDATA #REQUIRED>
]>
<users version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="users.xsd">
  <user id="1">
    <name>John Doe</name>
    <address>123 Main St, Anytown, USA</address>
    <phone>(123) 456-7890</phone>
  </user>
  <user id="2">
    <name>Jane Doe</name>
    <address>456 Elm St, Othertown, USA</address>
    <phone>(987) 654-3210</phone>
  </user>
</users>