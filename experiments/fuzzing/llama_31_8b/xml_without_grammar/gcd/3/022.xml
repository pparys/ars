<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE books [
  <!ELEMENT books (book+)> 
  <!ELEMENT book (author, title, price, publicationDate)> 
  <!ELEMENT author (#PCDATA)> 
  <!ELEMENT title (#PCDATA)> 
  <!ELEMENT price (#PCDATA)> 
  <!ELEMENT publicationDate (#PCDATA)>
  <!ATTLIST book 
    ISBN CDATA #REQUIRED
    publisher CDATA #IMPLIED>
]>
<books>
  <book ISBN="1234567890" publisher="ABC Publishing">
    <author>J. Doe</author>
    <title>example-title</title>
    <price>19.99</price>
    <publicationDate>2020-01-01</publicationDate>
  </book>
  <book ISBN="0987654321" publisher="XYZ Publishing">
    <author>Jane Smith</author>
    <title>another-example</title>
    <price>14.99</price>
    <publicationDate>2022-09-01</publicationDate>
  </book>
</books>