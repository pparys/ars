<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE customers [
  <!ELEMENT customers (customer*)>
  <!ELEMENT customer (name, address, contact)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (#PCDATA)>
  <!ELEMENT contact (#PCDATA)>
  <!ATTLIST customer
    id CDATA #REQUIRED>
]>
<customers>
  <customer id="12345">
    <name>John Doe</name>
    <address>123 Main St</address>
    <contact>555-1234</contact>
  </customer>
  <customer id="67890">
    <name>Jane Smith</name>
    <address>456 Elm St</address>
    <contact>555-5678</contact>
  </customer>
</customers>