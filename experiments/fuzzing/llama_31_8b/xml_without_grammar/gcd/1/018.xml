<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE customers [
  <!ELEMENT customers (customer*)>
  <!ELEMENT customer (name, address, phone)>
  <!ELEMENT name (#PCDATA)>
  <!ATTLIST address
    street CDATA #REQUIRED
    city CDATA #REQUIRED
    state CDATA #IMPLIED
    zip CDATA #IMPLIED>
  <!ATTLIST phone
    type CDATA #REQUIRED
    number CDATA #REQUIRED>
]>
<customers>
  <customer>
    <name>John Doe</name>
    <address street="123 Main St" city="Anytown" state="CA" zip="12345"/>
    <phone type="home" number="(555) 123-4567"/>
  </customer>
  <customer>
    <name>Jane Doe</name>
    <address street="456 Elm St" city="Othertown" state="NY" zip="67890"/>
    <phone type="work" number="(555) 901-2345"/>
  </customer>
</customers>