<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE employees [
  <!ELEMENT employees (employee*)>
  <!ELEMENT employee (name, address, phone)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST employee
    employee_id CDATA #REQUIRED
    manager_id CDATA #IMPLIED>
]>
<employees>
  <employee employee_id="1" manager_id="5">
    <name>John Doe</name>
    <address>
      <street>123 Main St</street>
      <city>Anytown</city>
      <state>CA</state>
      <zip>12345</zip>
    </address>
    <phone>555-555-5555</phone>
  </employee>
  <employee employee_id="2" manager_id="1">
    <name>Jane Doe</name>
    <address>
      <street>456 Elm St</street>
      <city>Othertown</city>
      <state>NY</state>
      <zip>67890</zip>
    </address>
    <phone>555-123-4567</phone>
  </employee>
</employees>