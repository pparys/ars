<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE customers [
  <!ELEMENT customers (customer*)>
  <!ELEMENT customer (name, address, contact)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT contact (#PCDATA)>
  <!ATTLIST customer
    id CDATA #REQUIRED>
]>
<customers>
  <customer id="1">
    <name>John Doe</name>
    <address>
      <street>123 Main St</street>
      <city>New York</city>
      <state>NY</state>
      <zip>10001</zip>
    </address>
    <contact>john.doe@example.com</contact>
  </customer>
  <customer id="2">
    <name>Jane Doe</name>
    <address>
      <street>456 Elm St</street>
      <city>Los Angeles</city>
      <state>CA</state>
      <zip>90001</zip>
    </address>
    <contact>jane.doe@example.com</contact>
  </customer>
</customers>