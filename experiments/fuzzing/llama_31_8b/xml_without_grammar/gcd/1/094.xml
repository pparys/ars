<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE person [
  <!ELEMENT person (name, address, contact)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT contact (#PCDATA)>
  <!ATTLIST person
    id CDATA #REQUIRED
    age CDATA #IMPLIED>
]>
<note>
  <person id="p123" age="30">
    <name>John Doe</name>
    <address>
      <street>123, Main St</street>
      <city>Anytown</city>
      <state>CA</state>
      <zip>12345</zip>
    </address>
    <contact>john.doe@example.com</contact>
  </person>
</note>