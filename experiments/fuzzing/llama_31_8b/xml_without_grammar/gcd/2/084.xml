<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE library [
  <!ELEMENT library (book+, author+)>
  <!ELEMENT book (author*, publisher*, price)>
  <!ATTLIST book
    id ID #REQUIRED
    title CDATA #REQUIRED>
  <!ATTLIST author
    id ID #REQUIRED
    name CDATA #REQUIRED>
  <!ATTLIST publisher
    name CDATA #REQUIRED>
  <!ATTLIST price
    amount CDATA #REQUIRED>
]>
