<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE addressbook [
  <!ELEMENT addressbook (name+, phone, email+)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ELEMENT email (#PCDATA)>
  <!ATTLIST addressbook
    type CDATA #IMPLIED
    owner CDATA #REQUIRED>
]>
<addressbook type="personal" owner="John Doe">
  <name>John Doe</name>
  <phone>123-456-7890</phone>
  <email>johndoe@example.com</email>
  <name>Jane Doe</name>
  <phone>098-765-4321</phone>
  <email>janedoe@example.com</email>
</addressbook>