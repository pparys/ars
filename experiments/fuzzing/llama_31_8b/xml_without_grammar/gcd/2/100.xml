<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
  <!ELEMENT root (person|address|phone)>
  <!ELEMENT person (name,age)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT age (#PCDATA)>
  <!ELEMENT address (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST root
    id CDATA #REQUIRED
    city CDATA #IMPLIED>
]>
<root id="12345" city="NYC">
  <person>
    <name>John Doe</name>
    <age>30</age>
  </person>
  <address>123 Main St</address>
  <phone>123-456-7890</phone>
</root>