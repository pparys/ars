<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE users [
  <!ELEMENT users (user+)>
  <!ELEMENT user (name, address, phone)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST user
    id ID #REQUIRED>
]>
