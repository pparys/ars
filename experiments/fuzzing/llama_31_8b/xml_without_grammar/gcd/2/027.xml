  <!ELEMENT item (name, description, price, quantity)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT description (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ELEMENT quantity (#PCDATA)>
  <!ATTLIST item
    id ID #REQUIRED>
]>
<inventory>
