<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE person [
  <!ELEMENT person (name, address, phone)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street, city, state)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST person
    id CDATA #REQUIRED
    age CDATA #IMPLIED
  >
]>
<person id="P001" age="25">
  <name>Jane Doe</name>
  <address>
    <street>123 Main St</street>
    <city>Anytown</city>
    <state>CA</state>
  </address>
  <phone>555-1234</phone>
</person>