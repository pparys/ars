<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
  <!ELEMENT root (customer,order*)>
  <!ELEMENT customer (name,address)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street,city,state,zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT order (date,item*)>
  <!ELEMENT date (#PCDATA)>
  <!ELEMENT item (product,quantity)>
  <!ELEMENT product (#PCDATA)>
  <!ELEMENT quantity (#PCDATA)>
  <!ATTLIST customer
    id CDATA #REQUIRED
    type CDATA #IMPLIED>
  <!ATTLIST order
    id CDATA #REQUIRED
    status CDATA #IMPLIED>
]>
<root>
  <customer id="123" type="individual">
    <name>John Doe</name>
    <address>
      <street>123 Main St</street>
      <city>Anytown</city>
      <state>CA</state>
      <zip>12345</zip>
    </address>
  </customer>
  <order id="456" status="shipped">
    <date>2024-07-17</date>
    <item>
      <product>Widget</product>
      <quantity>2</quantity>
    </item>
  </order>
  <order id="789" status="in stock">
    <date>2024-07-11</date>
    <item>
      <product>Gadget</product>
      <quantity>1</quantity>
    </item>
    <item>
      <product>Toy</product>
      <quantity>3</quantity>
    </item>
  </order>
</root>