<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
  <!ELEMENT root (person+)>
  <!ELEMENT person (address+, phone+ )>
  <!ELEMENT address (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST person
    id CDATA #IMPLIED
    name CDATA #IMPLIED
    age CDATA #IMPLIED
  >
  <!ATTLIST address
    street CDATA #IMPLIED
    city CDATA #IMPLIED
    state CDATA #IMPLIED
    zip CDATA #IMPLIED
  >
  <!ATTLIST phone
    type CDATA #IMPLIED
    number CDATA #REQUIRED
  >
]>
<root>
  <person id="1" name="John" age="30">
    <address street="123 Main St" city="New York" state="NY" zip="10001"/>
    <phone type="home" number="123-456-7890"/>
  </person>
  <person id="2" name="Jane" age="25">
    <address street="456 Elm St" city="Chicago" state="IL" zip="60611"/>
    <phone type="work" number="987-654-3210"/>
  </person>
</root>