<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE books [
  <!ELEMENT books (book+)>
  <!ELEMENT book (#PCDATA|author|price|title*)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ELEMENT title (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED
    year CDATA #IMPLIED>
]>
<books>
  <book id="1" year="2020">
    <title>Book 1</title>
    <price>$10.99</price>
    <author>Author 1</author>
  </book>
  <book id="2" year="2021">
    <title>Book 2</title>
    <price>$9.99</price>
    <author>Author 2</author>
  </book>
</books>