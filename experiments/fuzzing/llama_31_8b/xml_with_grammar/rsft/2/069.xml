<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE catalog [
  <!ELEMENT catalog (book*)>
  <!ELEMENT book (author, title, price)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED
    author-id CDATA #IMPLIED>
]>
<catalog>
    <book id="1" author-id="A1">
        <author>Andrew Jackson</author>
        <title>Python is Fun</title>
        <price>19.95</price>
    </book>
    <book id="2" author-id="A2">
        <author>George Washington</author>
        <title>History of the USA</title>
        <price>9.99</price>
    </book>
</catalog>