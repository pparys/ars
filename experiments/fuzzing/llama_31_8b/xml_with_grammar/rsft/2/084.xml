<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE users [
  <!ELEMENT users (user+)>
  <!ELEMENT user (#PCDATA | address | phone+)>
  <!ELEMENT address (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST users 
    name CDATA #IMPLIED>
  <!ATTLIST user 
    id CDATA #REQUIRED>
]>
<users name='John Doe'>
  <user id='1'>
    This is user 1
    <address>123 Main St</address>
    <phone>123-456-7890</phone>
  </user>
  <user id='2'>
    This is user 2
    <address>456 Elm St</address>
    <phone>987-654-3210</phone>
  </user>
</users>