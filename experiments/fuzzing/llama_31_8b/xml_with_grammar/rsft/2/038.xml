<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE customers [
  <!ELEMENT customers (customer*)>
  <!ELEMENT customer (#PCDATA | name | address | phone+, address?)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST customer
    id CDATA #REQUIRED
    name CDATA #IMPLIED>
  <!ATTLIST address
    street CDATA #REQUIRED
    city CDATA #REQUIRED
    state CDATA #IMPLIED
    zip CDATA #REQUIRED>
]>
<customers>
  <customer id="1" name="John Doe">
    <name>John Doe</name>
    <address street="123 Main St" city="Anytown" state="CA" zip="12345">
      <phone>123-456-7890</phone>
      <phone>098-765-4321</phone>
    </address>
  </customer>
  <customer id="2">
    <address street="456 Elm St" city="Othertown" zip="67890"/>
  </customer>
</customers>