<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE users [
  <!ELEMENT users (user+)>
  <!ELEMENT user (name, email, phone)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT email (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST users
    version CDATA #REQUIRED
  >
  <!ATTLIST user
    id CDATA #REQUIRED
  >
]>
<users version="1.1">
  <user id="1">
    <name>John Doe</name>
    <email>johndoe@example.com</email>
    <phone>123-456-7890</phone>
  </user>
  <user id="2">
    <name>Jane Doe</name>
    <email>janedoe@example.com</email>
    <phone>098-765-4321</phone>
  </user>
</users>