<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE peoples [
  <!ELEMENT peoples ((element)+ | (empty)+)>
  <!ELEMENT element (name, address, phone)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ELEMENT empty EMPTY>
  <!ATTLIST peoples
    title CDATA #IMPLIED>
  <!ATTLIST name
    type CDATA #REQUIRED>
]>
<peoples title="Peoples List">
  <element>
    <name type="john">John Doe</name>
    <address>av. America, 123</address>
    <phone>  +55 11 1234 5678</phone>
  </element>
  <element>
    <name type="jane">Jane Doe</name>
    <address>av. Europa, 456</address>
    <phone>  +55 11 9012 3456</phone>
  </element>
  <empty/>
</peoples>