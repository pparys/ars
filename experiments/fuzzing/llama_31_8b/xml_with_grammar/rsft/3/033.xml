<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE library [
  <!ELEMENT library (book+)>
  <!ELEMENT book (#PCDATA)>
  <!ATTLIST book
    author CDATA #IMPLIED
    year CDATA #REQUIRED
    price CDATA #IMPLIED>
]>
<library>
  <book author="John Doe" year="2020" price="19.99">This is a book</book>
  <book author="Jane Doe" year="2019" price="14.99">This is another book</book>
</library>