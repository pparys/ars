<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE calendar [
  <!ELEMENT calendar (month+)>
  <!ELEMENT month (day+)>
  <!ELEMENT day (#PCDATA)>
  <!ATTLIST calendar
    year CDATA #REQUIRED>
  <!ATTLIST month
    name CDATA #REQUIRED>
  <!ATTLIST day
    date CDATA #REQUIRED>
]>
<calendar year="2024">
  <month name="January">
    <day>1</day>
    <day>15</day>
  </month>
  <month name="February">
    <day>20</day>
    <day>24</day>
  </month>
</calendar>