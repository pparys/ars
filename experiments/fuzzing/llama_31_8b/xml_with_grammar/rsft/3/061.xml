        <address>123 Main St.</address>
        <address>Anytown, USA 12345</address>
    </customer>
    <customer id="2" name="Jane Doe">
        <address>456 Elm St.</address>
        <address>Othertown, USA 67890</address>
    </customer>
</catalog>