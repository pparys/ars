<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE addressBook [
  <!ELEMENT addressBook (person*)>
  <!ELEMENT person (name, phone, address)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ATTLIST person id CDATA #REQUIRED>
]>
<addressBook>
  <person id="1">
    <name>John Doe</name>
    <phone>123-456-7890</phone>
    <address>
      <street>123 Main St</street>
      <city>Anytown</city>
      <state>CA</state>
      <zip>12345</zip>
    </address>
  </person>
  <person id="2">
    <name>Jane Doe</name>
    <phone>987-654-3210</phone>
    <address>
      <street>456 Elm St</street>
      <city>Anytown</city>
      <state>NY</state>
      <zip>67890</zip>
    </address>
  </person>
</addressBook>