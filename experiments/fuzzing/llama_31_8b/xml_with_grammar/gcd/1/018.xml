<\xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE library [
  <!ELEMENT library (book+)>
  <!ELEMENT book (#PCDATA | author | title | price | publication-date)*>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ELEMENT publication-date (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED
    genre CDATA #IMPLIED>
]>
<library>
  <book id="bk101" genre="thriller">
    <author>John Doe</author>
    <title>Thriller</title>
    <price>$15.99</price>
    <publication-date>2000-12-01</publication-date>
  </book>
  <book id="bk102" genre="romance">
    <author>Jane Doe</author>
    <title>Romance</title>
    <price>$10.99</price>
    <publication-date>2001-03-01</publication-date>
  </book>
</library>