<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE user [
  <!ELEMENT user (name, address, contacts*)>
  <!ATTLIST user
    id ID #REQUIRED
    type CDATA #IMPLIED>
  <!ATTLIST address
    street CDATA #IMPLIED
    city CDATA #IMPLIED
    country CDATA #IMPLIED>
  <!ATTLIST contacts (phone, email)*>
  <!ATTLIST phone
    label CDATA #REQUIRED
    number CDATA #REQUIRED>
  <!ATTLIST email
    label CDATA #REQUIRED
    address CDATA #REQUIRED>
]>
<user id="123" type="customer">
  <name>John Doe</name>
  <address street="123 Main St" city="Anytown" country="USA"/>
  <contacts>
    <phone label="home" number="555-1234"/>
    <email label="work" address="johndoe@example.com"/>
  </contacts>
</user>