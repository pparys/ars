<\xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graph [
  <!ELEMENT graph (node+)>
  <!ATTLIST graph 
    name CDATA #REQUIRED>
  <!ATTLIST node 
    id CDATA #REQUIRED 
    x CDATA #IMPLIED 
    y CDATA #IMPLIED>
]>
<graph name="my graph">
  <node id="node1" x="10" y="20"/>
  <node id="node2" x="30" y="40"/>
  <node id="node3" x="50" y="60"/>
</graph>