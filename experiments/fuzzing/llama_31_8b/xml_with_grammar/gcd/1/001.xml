<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE addressbook [
  <!ELEMENT addressbook (address+)>
  <!ELEMENT address (name, phone)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST address
    street CDATA #IMPLIED
    city CDATA #IMPLIED
    state CDATA #IMPLIED
    zip CDATA #IMPLIED>
]>
<addressbook>
  <address street="1 Main St" city="Anytown" state="CA" zip="12345">
    <name>John Doe</name>
    <phone>123-456-7890</phone>
  </address>
  <address street="2 Elm St" city="Othertown" state="NY" zip="67890">
    <name>Jane Doe</name>
    <phone>987-654-3210</phone>
  </address>
</addressbook>