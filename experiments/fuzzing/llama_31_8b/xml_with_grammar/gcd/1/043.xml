<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE addressbook [
  <!ELEMENT addressbook (address*)>
  <!ELEMENT address (name, phone, email)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ELEMENT email (#PCDATA)>
  <!ATTLIST address 
    street CDATA #IMPLIED 
    city CDATA #IMPLIED>
]>
<addressbook>
  <address street="Main St" city="Anytown">
    <name>John Doe</name>
    <phone>123-4567</phone>
    <email>johndoe@example.com</email>
  </address>
  <address street=" Elm St" city="Othertown">
    <name>Jane Doe</name>
    <phone>765-4321</phone>
    <email>janedoe@example.com</email>
  </address>
</addressbook>