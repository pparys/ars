<\xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE data [
  <!ELEMENT data (person+)>
  <!ELEMENT person (name, age)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT age (#PCDATA)>
  <!ATTLIST person
    id CDATA #REQUIRED
  >
]>
<data>
  <person id="1">
    <name>John</name>
    <age>30</age>
  </person>
  <person id="2">
    <name>Jane</name>
    <age>25</age>
  </person>
</data>