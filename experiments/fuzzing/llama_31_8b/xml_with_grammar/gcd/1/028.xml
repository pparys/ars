<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE addressbook [
  <!ELEMENT addressbook (address+)>
  <!ELEMENT address (name,street,phone)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST address
    type (home|work) #REQUIRED
  >
]>
<addressbook>
  <address type="work">
    <name>John Smith</name>
    <street>123 Main St</street>
    <phone>123-456-7890</phone>
  </address>
  <address type="home">
    <name>Jane Doe</name>
    <street>456 Elm St</street>
    <phone>987-654-3210</phone>
  </address>
</addressbook>