<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE address [
  <!ELEMENT address (#PCDATA)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ATTLIST address
     type (home|work) "home"
     country (USA|CANADA) "USA"
  >
]>
<address>
  <name>Jane Doe</name>
  <street>123 Hello Drive</street>
  <city>Anytown</city>
  <state>CA</state>
  <zip>12345</zip>
</address>