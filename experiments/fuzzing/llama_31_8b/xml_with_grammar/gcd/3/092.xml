<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE books [
  <!ELEMENT books (author+,book+)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT book (title,publisher,year)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT publisher (#PCDATA)>
  <!ELEMENT year (#PCDATA)>
  <!ELEMENT chapter (#PCDATA)>
  <!ELEMENT publisher (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ELEMENT genre (#PCDATA)>
  <!ELEMENT author (#PCDATA)>
  <!ATTLIST author name CDATA #REQUIRED email CDATA #IMPLIED>
  <!ATTLIST book title CDATA #REQUIRED author CDATA #REQUIRED price CDATA #IMPLIED>
]>
<books>
    <author name="John Smith" email="john.smith@example.com"/>
    <book>
        <title>XML Tutorial</title>
        <publisher>Publishers</publisher>
        <year>2009</year>
        <chapter>Chapter 1</chapter>
        <price>19.99</price>
        <genre>Programming</genre>
    </book>
    <book>
        <title>XHTML Tutorial</title>
        <publisher>XML Publishers</publisher>
        <year>2014</year>
        <chapter>Introduction</chapter>
        <price>14.99</price>
        <genre>WTutorial</genre>
    </book>
    <author name="Jane Doe" email="jane.doe@example.com"/>
</books>