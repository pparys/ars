<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE address_book [
  <!ELEMENT address_book (address+)>
  <!ELEMENT address (name, phone, email)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ELEMENT email (#PCDATA)>
  <!ATTLIST address
    street CDATA #REQUIRED
    city CDATA #REQUIRED
    state CDATA #REQUIRED
  >
]>
<address_book>
  <address street="123 Main St" city="Anytown" state="NY">
    <name>John Doe</name>
    <phone>123-456-7890</phone>
    <email>johndoe@example.com</email>
  </address>
  <address street="456 Broadway" city="Othertown" state="CA">
    <name>Jane Smith</name>
    <phone>987-654-3210</phone>
    <email>janesmith@example.com</email>
  </address>
</address_book>