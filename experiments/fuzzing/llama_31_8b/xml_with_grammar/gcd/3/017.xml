<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE catalog [
  <!ELEMENT catalog ((book)+|picture+)>
  <!ELEMENT book (#PCDATA)>
  <!ELEMENT picture EMPTY>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ELEMENT picture_url (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED>
]>
<catalog>
  <book>
    <author>John Doe</author>
    <title>My Book</title>
    <price>19.99</price>
  </book>
  <book>
    <author>Jane Doe</author>
    <title>Another Book</title>
    <price>14.99</price>
  </book>
  <picture id="picture1" picture_url="https://example.com/picture.jpg"/>
  <picture id="picture2" picture_url="https://example.com/picture2.jpg"/>
</catalog>