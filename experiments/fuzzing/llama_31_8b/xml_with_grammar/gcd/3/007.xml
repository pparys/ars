<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE users [
  <!ELEMENT users (user+)>
  <!ELEMENT user (name, address)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ATTLIST user
    id CDATA #REQUIRED
    name CDATA #IMPLIED>
]>
<users>
  <user id="1" name="John Doe">
    <name>John Doe</name>
    <address>
      <street>123 Main St</street>
      <city>Anytown</city>
      <state>CA</state>
      <zip>12345</zip>
    </address>
  </user>
  <user id="2" name="Jane Smith">
    <name>Jane Smith</name>
    <address>
      <street>456 Elm St</street>
      <city>Othertown</city>
      <state>NY</state>
      <zip>67890</zip>
    </address>
  </user>
</users>