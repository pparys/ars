<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE address [
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ATTLIST address
    type (res|bus)"res"
    id ID #IMPLIED>
]>
<address type="res" id="addr-1">
  <street>123 Main Street</street>
  <city>Anytown</city>
  <state>CA</state>
  <zip>12345</zip>
</address>