<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE employees [
  <!ELEMENT employees (employee+)>
  <!ELEMENT employee (name, address)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (#PCDATA)>
  <!ATTLIST employee
    id CDATA #REQUIRED
    active (yes|no) #IMPLIED>
]>
<employees>
  <employee id="123" active="yes">
    <name>John Doe</name>
    <address>123 Main St, Anytown, USA</address>
  </employee>
  <employee id="456" active="no">
    <name>Jane Doe</name>
    <address>456 Elm St, Othertown, USA</address>
  </employee>
</employees>