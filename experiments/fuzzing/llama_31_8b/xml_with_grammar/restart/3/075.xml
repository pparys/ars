<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE addressbook [
  <!ELEMENT addressbook (person*)>
  <!ELEMENT person (#PCDATA)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT email (#PCDATA)>
  <!ATTLIST person
    id CDATA #REQUIRED>
]>
<addressbook>
  <person id="001">
    <name>John Doe</name>
    <email>johndoe@example.com</email>
  </person>
  <person id="002">
    <name>Jane Doe</name>
    <email>janedoe@example.com</email>
  </person>
</addressbook>