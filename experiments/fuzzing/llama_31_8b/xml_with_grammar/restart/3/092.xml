    date CDATA #REQUIRED
    total CDATA #IMPLIED>
]>
<purchaseOrder date="2024-07-26" total="1000.00">
  <shipTo street="123 Main St" city="Anytown" state="CA" zip="12345"/>
</purchaseOrder>