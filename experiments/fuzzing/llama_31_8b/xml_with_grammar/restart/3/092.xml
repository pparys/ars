<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE purchaseOrder [
  <!ELEMENT purchaseOrder (#PCDATA)>
  <!ELEMENT shipTo EMPTY>
  <!ATTLIST shipTo
    street CDATA #REQUIRED
    city CDATA #REQUIRED
    state CDATA #REQUIRED
    zip CDATA #REQUIRED>
