<\xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE library [
  <!ELEMENT library (book+)>
  <!ELEMENT book (title, author, publication-date)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT publication-date (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED
    genre CDATA #IMPLIED>
]>
<library>
  <book id="bk101" genre="novel">
    <title>My First Novel</title>
    <author>John Smith</author>
    <publication-date>1995-05-01</publication-date>
  </book>
  <book id="bk102" genre="biography">
    <title>My Life Story</title>
    <author>Jane Doe</author>
    <publication-date>2004-06-01</publication-date>
  </book>
</library>