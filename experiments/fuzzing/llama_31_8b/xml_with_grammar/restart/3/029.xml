<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE books [
  <!ELEMENT books (book+)>
  <!ELEMENT book (#PCDATA | title | author | price)+>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT price (#PCDATA)>
  <!ATTLIST book
    year CDATA #IMPLIED
    id CDATA #IMPLIED>
]>
<books>
  <book year="2020" id="1">
    <title>Book Title 1</title>
    <author>Author 1</author>
    <price>19.99</price>
  </book>
  <book year="2019" id="2">
    <title>Book Title 2</title>
    <author>Author 2</author>
    <price>29.99</price>
  </book>
</books>