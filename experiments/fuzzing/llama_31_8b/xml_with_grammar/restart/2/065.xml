<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE stock [
  <!ELEMENT stock (#PCDATA)>
  <!ELEMENT symbol (#PCDATA)>
  <!ELEMENT company (#PCDATA)>
  <!ELEMENT info  (symbol, company)>
  <!ELEMENT info_element EMPTY>
  <!ATTLIST stock
    type CDATA #REQUIRED
    year CDATA #IMPLIED>
  <!ATTLIST symbol
    color CDATA #REQUIRED>
  <!ATTLIST company
    address CDATA #IMPLIED
   -founded CDATA #IMPLIED>
]>
<stock type="equities" year="2023">
  <symbol color="red">
    <info symbol="AAPL">Apple Inc.</info>
    <info_element/>
  </symbol>
  <company>
    <address>123 Main St</address>
    <founded>1976</founded>
  </company>
</stock>