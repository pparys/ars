<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE customer [
  <!ELEMENT customer (name, address, order*)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT order (#PCDATA)>
  <!ATTLIST customer id CDATA #IMPLIED>
  <!ATTLIST order date CDATA #REQUIRED>
]>
<customer id="C123" xmlns="http://example.com/customer">
  <name>John Doe</name>
  <address>
    <street>123 Main St</street>
    <city>New York</city>
    <state>NY</state>
    <zip>10001</zip>
  </address>
  <order date="2024-07-25">Order #1</order>
  <order date="2024-07-30">Order #2</order>
</customer>