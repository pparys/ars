<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE library [
  <!ELEMENT library (book+)>
  <!ELEMENT book (title, author, year)>
  <!ELEMENT title (#PCDATA)>
  <!ELEMENT author (#PCDATA)>
  <!ELEMENT year (#PCDATA)>
  <!ELEMENT address (#PCDATA)>
  <!ATTLIST book
    id CDATA #REQUIRED
    price CDATA #IMPLIED>
]>
<library>
  <book id="BK1" price="19.99">
    <title>My Book</title>
    <author>John Doe</author>
    <year>2022</year>
    <address>123 Main St, CA 94025</address>
  </book>
  <book id="BK2" price="14.99">
    <title>Your Book</title>
    <author>Jane Smith</author>
    <year>2021</year>
    <address>456 Elm St, NY 10001</address>
  </book>
</library>