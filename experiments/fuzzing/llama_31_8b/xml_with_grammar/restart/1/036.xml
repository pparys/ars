<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE user [
  <!ELEMENT user (name,address,email)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street,city,state)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT email (#PCDATA)>
  <!ATTLIST user id CDATA #REQUIRED>
]>
<user id="12345">
  <name>John Doe</name>
  <address>
    <street>123 Main St</street>
    <city>Anytown</city>
    <state>CA</state>
  </address>
  <email>johndoe@example.com</email>
</user>