<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE students [
  <!ELEMENT students (student+)> 
  <!ELEMENT student (name,score+,courses+)> 
  <!ELEMENT name (#PCDATA)> 
  <!ELEMENT score (#PCDATA)> 
  <!ELEMENT courses (course+)> 
  <!ELEMENT course (#PCDATA)> 

  <!ATTLIST student
    id CDATA #REQUIRED
    age CDATA #IMPLIED
  > 
  <!ATTLIST courses
    major CDATA #REQUIRED
  > 
]>
<students>
  <student id="s1" age="20">
    <name>John Doe</name>
    <score>85</score>
    <score>90</score>
    <courses major="CS">Data Structures</courses>
    <courses major="CS">Algorithms</courses>
  </student>
  <student id="s2" age="22">
    <name>Jane Doe</name>
    <score>95</score>
    <courses major="Math">Calculus</courses>
    <courses major="Math">Linear Algebra</courses>
  </student>
</students>