<\xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE calendar [
  <!ELEMENT calendar (year+)>
  <!ELEMENT year (month+)>
  <!ELEMENT month (day+)>
  <!ELEMENT day (#PCDATA)>
  <!ATTLIST day date CDATA #REQUIRED>
]>
<calendar>
  <year>
    <month>
      <day date="2022-01-01">January 1</day>
    </month>
    <month>
      <day date="2022-01-02">January 2</day>
    </month>
  </year>
  <year>
    <month>
      <day date="2023-01-01">January 1</day>
    </month>
    <month>
      <day date="2023-01-02">January 2</day>
    </month>
  </year>
</calendar>