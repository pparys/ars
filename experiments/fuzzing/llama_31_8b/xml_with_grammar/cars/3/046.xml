<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE employee [
  <!ELEMENT employee (name,salary,department)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT salary (#PCDATA)>
  <!ELEMENT department (#PCDATA)>
  <!ELEMENT address (street,city,state,country)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT country (#PCDATA)>
  <!ATTLIST employee
    id CDATA #IMPLIED>
]>
<employee id="E101">
    <name>John Doe</name>
    <salary>$80.00</salary>
    <department>HR</department>
    <address>
        <street>123 Main St</street>
        <city>New York</city>
        <state>NY</state>
        <country>USA</country>
    </address>
</employee>