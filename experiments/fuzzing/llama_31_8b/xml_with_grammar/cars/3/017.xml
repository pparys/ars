<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE student [
  <!ELEMENT student (name|martial_status|phone)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT martial_status (#PCDATA)>
  <!ELEMENT address (street|city|state|zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ATTLIST student
    id CDATA #REQUIRED
    grad_year CDATA #IMPLIED>
]>
<student id="S1234" grad_year="2025">
  <name>John Doe</name>
  <address>
    <street>Main Street</street>
    <city>Anytown</city>
    <state>CA</state>
    <zip>12345</zip>
  </address>
  <phone>555-1234</phone>
</student>