<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE purchaseOrder [
  <!ELEMENT purchaseOrder (name,address lineno+)>
  <!ELEMENT name (firstName,lastName)>
  <!ELEMENT address (street, state,zip)>
  <!ELEMENT item (product,quantity,unitPrice)>
  <!ELEMENT product (#PCDATA)>
  <!ELEMENT lineno (#PCDATA)>
  <!ELEMENT firstName (#PCDATA)>
  <!ELEMENT lastName (#PCDATA)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ATTLIST lineno
    LINENO CDATA #REQUIRED
  >
  <!ATTLIST item
    lineNumber CDATA #IMPLIED
  >
]>
<purchaseOrder>
  <name>
    <firstName>John</firstName>
    <lastName>Doe</lastName>
  </name>
  <address>
    <street>123 Main St</street>
    <state>NY</state>
    <zip>12345</zip>
  </address>
  <lineno LINENO="1"/>
  <item lineNumber="1">
    <product>Book</product>
    <quantity>2</quantity>
    <unitPrice>10</unitPrice>
  </item>
  <item lineNumber="2">
    <product>Pencil</product>
    <quantity>1</quantity>
    <unitPrice>5</unitPrice>
  </item>
</purchaseOrder>