<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE addressbook [
  <!ELEMENT addressbook (personaladdress+)>
  <!ELEMENT personaladdress (name, address)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT address (street, city, state, zip)>
  <!ELEMENT street (#PCDATA)>
  <!ELEMENT city (#PCDATA)>
  <!ELEMENT state (#PCDATA)>
  <!ELEMENT zip (#PCDATA)>
  <!ATTLIST personaladdress
    contactname CDATA #REQUIRED
    phone CDATA #IMPLIED>
]>
<addressbook>
  <personaladdress contactname="John Doe" phone="123-456-7890">
    <name>John Doe</name>
    <address><street>123 Main St</street><city>Anytown</city><state>CA</state><zip>12345</zip></address>
  </personaladdress>
</addressbook>