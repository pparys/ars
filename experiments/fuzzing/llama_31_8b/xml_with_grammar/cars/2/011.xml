<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
  <!ELEMENT root (message*, person*)>
  <!ELEMENT message (#PCDATA)>
  <!ELEMENT person (name, phone*, address)>
  <!ELEMENT name (#PCDATA)>
  <!ELEMENT phone (#PCDATA)>
  <!ELEMENT address (#PCDATA)>
  <!ATTLIST root
    producer CDATA #REQUIRED
    version CDATA #REQUIRED>
  <!ATTLIST message
    id CDATA #REQUIRED
    type CDATA #IMPLIED>
  <!ATTLIST person
    id CDATA #REQUIRED
    age CDATA #IMPLIED>
]>
<root producer="ABC Corporation" version="1.0">
  <message id="1" type="Error">System is down.</message>
  <message id="2" type="Warning">System is busy.</message>
  <message id="3">System is idle.</message>
  <person id="1" age="30">
    <name>John Doe</name>
    <phone>123-456-7890</phone>
    <phone>098-765-4321</phone>
    <address>123 Main St</address>
  </person>
  <person id="2" age="25">
    <name>Jane Doe</name>
    <phone>555-123-4567</phone>
    <address>456 Elm St</address>
  </person>
</root>