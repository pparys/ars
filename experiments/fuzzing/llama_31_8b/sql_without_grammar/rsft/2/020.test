set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-1.0 {
  execsql {
    CREATE TABLE products(category TEXT, name TEXT PRIMARY KEY, price REAL);
    INSERT INTO products VALUES('Electronics', 'Laptop', 1200.99);
    INSERT INTO products VALUES('Electronics', 'Phone', 599.99);
  }
} {}

do_test get_specific_product-1.1 {
  execsql {
    SELECT name, price FROM products WHERE category = 'Electronics' AND name = 'Phone';
  }
} {Phone 599.99}

finish_test