set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-autoincrement-1.0 {
  execsql {
    CREATE TABLE patients(id INTEGER PRIMARY KEY AUTOINCREMENT, first_name TEXT, last_name TEXT);
    INSERT INTO patients(first_name, last_name) VALUES('John', 'Doe');
    INSERT INTO patients(first_name, last_name) VALUES('Jane', 'Doe');
  }
} {}

do_test select-foreign-key-1.1 {
  execsql {
    SELECT * FROM patients WHERE id = 1;
  }
} {1 John Doe}

finish_test