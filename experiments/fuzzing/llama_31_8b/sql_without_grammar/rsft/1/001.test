set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-products-2.0 {
  execsql {
    CREATE TABLE products(id INTEGER PRIMARY KEY, name TEXT, price REAL);
    INSERT INTO products VALUES(1, 'Product A', 10.99);
    INSERT INTO products VALUES(2, 'Product B', 5.99);
    INSERT INTO products VALUES(3, 'Product C', 7.99);
  }
} {}

do_test select-product-price-2.1 {
  execsql {
    SELECT price FROM products WHERE name = 'Product B' AND price > 5;
  }
} {7.99}

finish_test