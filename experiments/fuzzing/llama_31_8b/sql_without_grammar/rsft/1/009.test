set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-users-2.0 {
  execsql {
    CREATE TABLE users(id INTEGER PRIMARY KEY, name TEXT, age INTEGER);
    INSERT INTO users(id, name, age) VALUES(201, 'Alice', 25);
    INSERT INTO users(id, name, age) VALUES(202, 'Bob', 30);
  }
} {}

do_test select-users-2.1 {
  execsql {
    SELECT name, age FROM users WHERE age > 25;
  }
} {Bob 30}

finish_test