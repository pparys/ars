set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-unittest-1.0 {
  execsql {
    CREATE TABLE unit_tests(id INTEGER PRIMARY KEY, name TEXT, result TEXT);
    INSERT INTO unit_tests(id, name, result) VALUES(1, 'test1', 'ok');
    INSERT INTO unit_tests(id, name, result) VALUES(2, 'test2', 'fail');
  }
} {}

do_test query-expected-result-1.1 {
  execsql {
    SELECT name, result FROM unit_tests WHERE id = 1;
  }
} {test1 ok}

finish_test