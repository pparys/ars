  execsql {
    CREATE TABLE actions(id INTEGER, description TEXT, type TEXT);
    INSERT INTO actions(id, description, type) VALUES(1, 'Action 1', 'type1');
    INSERT INTO actions(id, description, type) VALUES(2, 'Action 2', 'type2');
