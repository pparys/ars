set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-clause-1.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, customer TEXT, order_date TEXT);
    INSERT INTO orders VALUES(101, 'Alice', '2022-01-01');
    INSERT INTO orders VALUES(102, 'Bob', '2022-01-02');
    INSERT INTO orders VALUES(103, 'Alice', '2022-01-03');
  }
} {}

do_test order-bymultiple-1.1 {
  execsql {
    SELECT customer, order_date FROM orders ORDER BY customer DESC, order_date ASC;
  }
} {Alice 2022-01-03 Bob 2022-01-02}

finish_test