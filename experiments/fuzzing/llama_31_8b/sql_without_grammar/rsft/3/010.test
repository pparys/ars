set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test insert-ignore-null-3.0 {
  execsql {
    CREATE TABLE orders(id INTEGER PRIMARY KEY, customer_name TEXT);
    INSERT INTO orders VALUES(101, 'John Doe');
    INSERT INTO orders(customer_name) VALUES('Jane Doe');
  }
} {}

do_test select-orders-3.1 {
  execsql {
    SELECT * FROM orders;
  }
} {101 John Doe 2 Jane Doe}

finish_test