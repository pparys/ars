set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-before-1.0 {
  execsql {
    CREATE TABLE holidays(month INTEGER, day INTEGER);
    INSERT INTO holidays VALUES(1, 1);
    INSERT INTO holidays VALUES(1, 2);
    INSERT INTO holidays VALUES(1, 3);
  }
} {}

do_test select-holiday-2.0 {
  execsql {
    SELECT day FROM holidays WHERE month = 1 ORDER BY day DESC LIMIT 1;
  }
} {1}

finish_test