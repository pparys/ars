set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-orders-1.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, customer_id INTEGER, product TEXT, quantity INTEGER);
    INSERT INTO orders(id, customer_id, product, quantity) VALUES(121, 1, 'Laptop', 1);
    INSERT INTO orders(id, customer_id, product, quantity) VALUES(122, 1, 'Mouse', 2);
    INSERT INTO orders(id, customer_id, product, quantity) VALUES(123, 2, 'Keyboard', 3);
  }
} {}

do_test retrieve-orders-1.1 {
  execsql {
    SELECT product, quantity FROM orders WHERE customer_id = 1 ORDER BY id DESC;
  }
} {Laptop 1 Mouse 2}

finish_test