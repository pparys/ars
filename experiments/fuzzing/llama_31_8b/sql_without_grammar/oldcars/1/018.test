set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-queries-1.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER PRIMARY KEY, customer_name TEXT NOT NULL, order_date DATE);
    INSERT INTO orders(order_id, customer_name, order_date) VALUES(1001, 'Jon Doe', '2022-05-01');
    INSERT INTO orders(order_id, customer_name, order_date) VALUES(1002, 'Jane Doe', '2022-06-01');
  }
} {}

do_test select-orders-1.1 {
  execsql {
    SELECT customer_name, order_date FROM orders;
  }
} {Jon Doe 2022-05-01 Jane Doe 2022-06-01}

finish_test