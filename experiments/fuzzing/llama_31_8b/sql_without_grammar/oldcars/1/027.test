set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-1.0 {
  execsql {
    CREATE TABLE employee(id INTEGER PRIMARY KEY, name TEXT, salary REAL);
    INSERT INTO employee VALUES(1, 'John Doe', 50000.0);
    INSERT INTO employee VALUES(2, 'Jane Doe', 60000.0);
  }
} {}

do_test select-larger-salary-1.1 {
  execsql {
    SELECT name, salary FROM employee ORDER BY salary DESC LIMIT 1;
  }
} {Jane Doe 60000.0}

finish_test