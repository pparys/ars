set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-3.0 {
  execsql {
    CREATE TABLE orders(id INTEGER, customer_name TEXT, order_date DATE);
    INSERT INTO orders VALUES(1001, 'Michael Bolton', '2022-01-01');
    INSERT INTO orders VALUES(1002, 'LISA FERGUSON', '2022-01-15');
  }
} {}

do_test order-before-max-3.1 {
  execsql {
    SELECT order_date FROM orders WHERE id < 1003
    ORDER BY order_date DESC LIMIT 1;
  }
} {2022-01-15}

finish_test