set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-primary-key-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER PRIMARY KEY, customer_id INTEGER, total REAL);
    INSERT INTO orders VALUES(1001, 1, 10.99);
    INSERT INTO orders VALUES(1002, 2, 9.99);
  }
} {}

do_test retrieve-order-3.1 {
  execsql {
    SELECT order_id, customer_id FROM orders WHERE order_id = 1001;
  }
} {1001 1}

finish_test