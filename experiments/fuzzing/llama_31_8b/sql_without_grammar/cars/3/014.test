set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-employees-1.0 {
  execsql {
    CREATE TABLE employees(id INTEGER PRIMARY KEY, name TEXT, department TEXT);
    INSERT INTO employees(id, name, department) VALUES(201, 'John Doe', 'Sales');
    INSERT INTO employees(id, name, department) VALUES(202, 'Jane Doe', 'Marketing');
  }
} {}

do_test query-employees-1.1 {
  execsql {
    SELECT department FROM employees ORDER BY department;
  }
} {Marketing Sales}

finish_test