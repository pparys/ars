set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-tables-1.0 {
  execsql {
    CREATE TABLE orders(id INTEGER PRIMARY KEY AUTOINCREMENT, customer_id INTEGER);
    CREATE TABLE customers(id INTEGER PRIMARY KEY AUTOINCREMENT, name TEXT UNIQUE);
    INSERT INTO customers VALUES(1, 'John');
    INSERT INTO customers VALUES(2, 'Jane');
    INSERT INTO orders(customer_id) VALUES(1);
  }
} {}

do_test select-orders-1.1 {
  execsql {
    SELECT id, customer_id FROM orders WHERE customer_id = 1;
  }
} {1 1}

finish_test