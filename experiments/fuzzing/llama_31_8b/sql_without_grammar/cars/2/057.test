set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-order-3.0 {
  execsql {
    CREATE TABLE products(id INTEGER PRIMARY KEY, order_id INTEGER, price REAL);
    INSERT INTO products(id, order_id, price) VALUES(101, 1001, 9.99);
    INSERT INTO products(id, order_id, price) VALUES(102, 1001, 19.99);
    INSERT INTO products(id, order_id, price) VALUES(103, 1002, 29.99);
  }
} {}

do_test select-expensive-products-3.1 {
  execsql {
    SELECT price FROM products WHERE price > 19.99 ORDER BY id DESC;
  }
} {29.99 19.99}

finish_test