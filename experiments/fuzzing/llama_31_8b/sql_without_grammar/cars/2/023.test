set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-employees-1.0 {
  execsql {
    CREATE TABLE employees(id INTEGER PRIMARY KEY, name TEXT, salary REAL);
    INSERT INTO employees(id, name, salary) VALUES(201, 'John Doe', 50000.0);
    INSERT INTO employees(id, name, salary) VALUES(202, 'Jane Doe', 60000.0);
  }
} {}

do_test get-employee-salary-1.1 {
  execsql {
    SELECT name, salary FROM employees WHERE id = 202;
  }
} {Jane Doe 60000.0}

finish_test