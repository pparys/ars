set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-store-3.0 {
  execsql {
    CREATE TABLE products(product_id INTEGER PRIMARY KEY, product_name TEXT NOT NULL, supplier_id INTEGER);
    INSERT INTO products(product_id, product_name, supplier_id) VALUES(1, 'Product A', 1);
    INSERT INTO products(product_id, product_name, supplier_id) VALUES(2, 'Product B', 2);
    INSERT INTO products(product_id, product_name, supplier_id) VALUES(3, 'Product C', 1);
  }
} {}

do_test select-product-store-3.1 {
  execsql {
    SELECT product_name, supplier_id FROM products WHERE product_id = 2;
  }
} {Product B 2}

finish_test