set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE products(id INTEGER PRIMARY KEY AUTOINCREMENT, name TEXT, price REAL);
    INSERT INTO products(name, price) VALUES('Product A', 19.99);
    INSERT INTO products(name, price) VALUES('Product B', 9.99);
  }
} {}

do_test select-product-2.1 {
  execsql {
    SELECT name, price FROM products WHERE id = 2;
  }
} {Product B 9.99}

finish_test