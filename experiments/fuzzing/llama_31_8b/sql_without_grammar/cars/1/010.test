set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test insert-into-table-3.0 {
  execsql {
    CREATE TABLE products(id INTEGER PRIMARY KEY, name TEXT NOT NULL, price REAL);
    INSERT INTO products(name, price) VALUES('Product A', 10.99);
    INSERT INTO products(name, price) VALUES('Product B', 9.99);
  }
} {}

do_test get-latest-product-3.1 {
  execsql {
    SELECT name, price FROM products ORDER BY id DESC LIMIT 1;
  }
} {Product B 9.99}

finish_test