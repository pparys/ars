set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-2.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER PRIMARY KEY, customer_id INTEGER, total REAL);
    INSERT INTO orders(order_id, customer_id, total) VALUES(201, 301, 100.0);
    INSERT INTO orders(order_id, customer_id, total) VALUES(202, 302, 200.0);
  }
} {}

