set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-items-1.0 {
  execsql {
    CREATE TABLE items(id INTEGER PRIMARY KEY, name TEXT, quantity INTEGER);
    INSERT INTO items VALUES(1, 'Apple', 5);
    INSERT INTO items VALUES(2, 'Banana', 10);
    INSERT INTO items VALUES(3, 'Cherry', 7);
  }
} {}

do_test update-quantity-1.1 {
  execsql {
    UPDATE items SET quantity = 15 WHERE id = 1;
  }
} {}

finish_test