set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-transaction-3.0 {
  execsql {
    CREATE TABLE logs(
      id INTEGER PRIMARY KEY AUTOINCREMENT,
      timestamp TEXT DEFAULT CURRENT_TIMESTAMP,
      type TEXT NOT NULL
    );
    INSERT INTO logs(type) VALUES('INFO');
    INSERT INTO logs(type) VALUES('WARNING');
  }
} {}

do_test rollback-transaction-3.1 {
  execsql {
    UPDATE logs SET type = 'CRITICAL';
    UPDATE logs SET type = 'INFO';
  }
} {1 {no transaction is active}                                                                                  
                                                                                         											  												  																			 
          																														  																		
											 	finish_test