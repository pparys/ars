set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-calendars-3.1 {
  execsql {
    CREATE TABLE calendars(id INTEGER PRIMARY KEY, name TEXT);
    INSERT INTO calendars(id, name) VALUES(1, 'Personal');
    INSERT INTO calendars(id, name) VALUES(2, 'Work');
  }
} {}

do_test select-calendars-3.2 {
  execsql {
    SELECT name FROM calendars WHERE id = 1;
  }
} {Personal}

finish_test