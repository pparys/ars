set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-tables-3.0 {
  execsql {
    CREATE TABLE customers(id INTEGER, name TEXT, email TEXT);
    CREATE TABLE orders(id INTEGER, customer_id INTEGER, total REAL);
    INSERT INTO customers(id, name, email) VALUES(1, 'John Doe', 'john@example.com');
    INSERT INTO customers(id, name, email) VALUES(2, 'Jane Doe', 'jane@example.com');
    INSERT INTO orders(id, customer_id, total) VALUES(101, 1, 10.99);
    INSERT INTO orders(id, customer_id, total) VALUES(102, 2, 5.99);
  }
} {}

do_test insert-test-3.1 {
  execsql {
    SELECT * FROM customers WHERE id = 2;
  }
} {2 Jane Doe jane@example.com}

finish_test