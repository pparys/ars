set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-foreign-key-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER PRIMARY KEY);
    CREATE TABLE customers(customer_id INTEGER PRIMARY KEY, order_id INTEGER, FOREIGN KEY order_id REFERENCES orders);
    INSERT INTO orders VALUES(1);
    INSERT INTO customers VALUES(1, 1);
  }
} {}

do_test insert-violates-foreign-key-3.1 {
  execsql {
    SELECT customer_id FROM customers;
  }
} {1}

finish_test