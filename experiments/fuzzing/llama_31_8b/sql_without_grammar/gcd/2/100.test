set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-orders-3.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER, customer_name TEXT, total REAL);
    INSERT INTO orders(order_id, customer_name, total) VALUES(1, 'John Doe', 100.99);
    INSERT INTO orders(order_id, customer_name, total) VALUES(2, 'Jane Doe', 200.99);
    INSERT INTO orders(order_id, customer_name, total) VALUES(101, 'Jim Doe', 50.99);
  }
} {}

do_test delete-order-3.1 {
  execsql {
    DELETE FROM orders WHERE customer_name = 'Jane Doe';
  }
} {}
finish_test