    INSERT INTO customers VALUES(2, 'Jane Doe', 'jane@example.com');
    INSERT INTO orders VALUES(101, 1, '2022-01-01');
    INSERT INTO orders VALUES(102, 2, '2022-01-15');
  }
} {}

