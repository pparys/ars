set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-transactions-1.0 {
  execsql {
    CREATE TABLE orders(id INTEGER PRIMARY KEY, customer_id INTEGER);
    INSERT INTO orders(id, customer_id) VALUES(1, 101);
    INSERT INTO orders(id, customer_id) VALUES(2, 102);
  }
} {}

do_test rollback-transaction-1.1 {
  execsql {
    UPDATE orders SET customer_id = 102 WHERE id = 1;
    UPDATE orders SET customer_id = 101 WHERE id = 2;
    SELECT * FROM orders;
  }
} {}

finish_test