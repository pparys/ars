set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-data-3.0 {
  execsql {
    CREATE TABLE employees(id INTEGER PRIMARY KEY, name TEXT, salary REAL);
    INSERT INTO employees VALUES(1001, 'John Smith', 50000.0);
    INSERT INTO employees VALUES(1002, 'Jane Doe', 60000.0);
    INSERT INTO employees VALUES(1003, 'Bob Johnson', 55000.0);
  }
} {}

do_test view-result-3.1 {
  execsql {
    SELECT name, salary FROM employees
    WHERE salary > 55000
    ORDER BY salary DESC
  ;}
} {Jane Doe 60000.0 John Smith 50000.0}

finish_test