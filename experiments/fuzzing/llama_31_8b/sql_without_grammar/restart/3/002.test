set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-tables-3.0 {
  execsql {
    CREATE TABLE company (
      id INTEGER PRIMARY KEY,
      name TEXT,
      address TEXT
    );
    CREATE TABLE employee (
      id INTEGER PRIMARY KEY,
      name TEXT,
      email TEXT,
      company_id INTEGER,
      FOREIGN KEY company_id REFERENCES company
    );
    INSERT INTO company VALUES(1, 'Acme Inc', '123 Main St');
    INSERT INTO employee VALUES(1, 'John Doe', 'johndoe@example.com', 1);
    INSERT INTO employee VALUES(2, 'Jane Doe', 'janedoe@example.com', 1);
  }
} {}

do_test insert-violates-foreign-key-3.1 {
  execsql {
    UPDATE company SET address = '456 Park Ave';
  }
  	

  	} {}
  	      
       	
  																			
  							
  		                   											 
  																																																								
  												  																		
 
   															            								
  														                                                                                                                              
  																				  
  									
  																				 
  																				 
  																																												 
  																				 
  																				 
  																				 
  											
  																		  							

  								                    
 
 							   																			    
  																													                                                                									
 				                                                          									
 															                                                                                                                                                                                   
  																		
  											
  							 
  							     
  							     
  							     
 																		 

 						  
  			
 
     																														   
  										
  									
  																				 

  														            
  																				
  													 
  																		  
  																		
  																						                               		                                                                                                                                        
  																			 
  																																									   
  																		  	
  																										      				                                         
  	                                    		                                                                      
 																						    
  									    		
  													 
         
  							 
  																				 
  	  		
  											
  																		  
  																		
  				
 							 												      
  																		          
  																
  																				   
  																				 
  	
  										
  								 
  														  		                                                                                                                                						  
  									
                                                    					    	                 			  
   
      	                                 	 												                                                                                                                                	
      							
  																
                              													 											 
                                                    											  																							 	 															