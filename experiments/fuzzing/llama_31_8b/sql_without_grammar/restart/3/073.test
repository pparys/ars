set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-temp-table-3.0 {
  execsql {
    CREATE TABLE artists(id INTEGER, name TEXT);
    INSERT INTO artists(id, name) VALUES(301, 'Rihanna');
