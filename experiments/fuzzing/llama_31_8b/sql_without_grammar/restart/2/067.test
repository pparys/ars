set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup_threads-1.0 {
  execsql {
    CREATE TABLE threads(id INTEGER PRIMARY KEY AUTOINCREMENT, name TEXT);
    INSERT INTO threads(name) VALUES('Thread 1');
    INSERT INTO threads(name) VALUES('Thread 2');
  }
} {}

do_test select_thread-1.1 {
  execsql {
    SELECT name FROM threads WHERE id = 1;
  }
} {'Thread 1'}

finish_test