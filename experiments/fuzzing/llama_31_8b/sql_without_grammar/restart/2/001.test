set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test setup-table-1.0 {
  execsql {
    CREATE TABLE customers(id INTEGER PRIMARY KEY, name TEXT, age INT);
    CREATE TABLE orders(id INTEGER PRIMARY KEY, customer_id INTEGER, FOREIGN KEY customer_id REFERENCES customersid);
  CREATE TABLE items(id INTEGER PRIMARY KEY, name TEXT, price REAL);
  INSERT INTO customers(id, name, age) VALUES(1, 'John', 30);
  INSERT INTO customers(id, name, age) VALUES(2, 'Alice', 25);
  INSERT INTO orders(id, customer_id) VALUES(1, 1);
  INSERT INTO items(id, name, price) VALUES(101, 'Apple', 0.99);
}
} {}

do_test insert-data-1.1 {
  execsql {
    SELECT c.* FROM customers AS c
    ORDER BY c_name;
  }
} {1 John 30 2 Alice 25}

finish_test