set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test part-integer-div-1.0 {
  execsql {
    CREATE TABLE parts(id INTEGER, count INTEGER);
    INSERT INTO parts(id, count) VALUES(1, 10);
