set testdir [file dirname $argv0]
source $testdir/tester.tcl

do_test set-foreign-1.0 {
  execsql {
    CREATE TABLE orders(order_id INTEGER PRIMARY KEY, customer_id INTEGER);
    CREATE TABLE customers(customer_id INTEGER PRIMARY KEY, name TEXT);
    INSERT INTO customers(customer_id, name) VALUES(1, 'John');
    INSERT INTO customers(customer_id, name) VALUES(2, 'Jane');
    INSERT INTO orders(order_id, customer_id) VALUES(1, 1);
    INSERT INTO orders(order_id, customer_id) VALUES(2, 2);
  }
} {}

do_test get-orders-1.1 {
  execsql {
    SELECT * FROM orders;
  }
} {1 1 2 2}

finish_test