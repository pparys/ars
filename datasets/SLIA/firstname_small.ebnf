root ::= "(define-fun f ((name String)) String " Start_ ")"
Start_ ::= ntString
ntString ::= "name" | "\" \"" | "(" "str.++" " " ntString " " ntString ")" | "(" "str.replace" " " ntString " " ntString " " ntString ")" | "(" "str.at" " " ntString " " ntInt ")" | "(" "str.substr" " " ntString " " ntInt " " ntInt ")"
ntInt ::= "0" | "1" | "2" | "(" "+" " " ntInt " " ntInt ")" | "(" "-" " " ntInt " " ntInt ")" | "(" "str.len" " " ntString ")" | "(" "str.indexof" " " ntString " " ntString " " ntInt ")"
ntBool ::= "true" | "false" | "(" "str.prefixof" " " ntString " " ntString ")" | "(" "str.suffixof" " " ntString " " ntString ")"

